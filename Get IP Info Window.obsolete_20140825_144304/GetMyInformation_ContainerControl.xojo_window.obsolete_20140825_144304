#tag WindowCode
	#tag Method, Flags = &h0
		Sub mPushMyInfoToWindow()
		  Dim NumberOfDictionaryEntries as Integer = GetMYInfo_Dictionary.Count-1
		  
		  For i as integer = 0 to NumberOfDictionaryEntries
		    if i = 0 then IPInfo_Window.GetMyInformation_ContainerControl1.MyIPAddress_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		    if i = 1 then IPInfo_Window.GetMyInformation_ContainerControl1.MyHostname_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		    if i = 2 then IPInfo_Window.GetMyInformation_ContainerControl1.MyCity_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		    if i = 3 then IPInfo_Window.GetMyInformation_ContainerControl1.MyRegion_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		    if i = 4 then IPInfo_Window.GetMyInformation_ContainerControl1.MyCountry_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		    if i = 5 then IPInfo_Window.GetMyInformation_ContainerControl1.MyGeoPos_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		    if i = 6 then IPInfo_Window.GetMyInformation_ContainerControl1.MyISP_TextField.Text = GetMYInfo_Dictionary.Value(GetMYInfo_Dictionary.key(i))
		  Next i
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		GetMYInfo_Dictionary As Dictionary
	#tag EndProperty

	#tag Property, Flags = &h21
		Private GoogleMAP_API As GoogleMAPsAPI_Class
	#tag EndProperty


#tag EndWindowCode

