		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(97) to Chr(122)
		    Tooltip.Show("Sorry, IPv4 addresses only allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(123)
		    Tooltip.Show("Sorry, Open Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(124)
		    Tooltip.Show("Sorry, Pipe not allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(125)
		    Tooltip.Show("Sorry, Closed Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(126)
		    Tooltip.Show("Sorry, Grave Accent Marks not allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(127)
		    Tooltip.Show("Sorry, IPv4 addresses only allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		  End Select
		  
		  
		  
		  
		End Function
	#tag EndEvent
#tag EndEvents
