		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(92)
		    Tooltip.Show("Sorry, Back Slash not allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(93)
		    Tooltip.Show("Sorry, Closed Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    mClearAllFields
		    Return True
		    
		  Case Chr(94)
