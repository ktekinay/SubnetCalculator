#tag Events SearchButton
	#tag Event
		Sub Action()
		  if EnterIP_Search_TextField.Text = "" Then Return
		  
		  Dim IPV4_Validation as String = fIPv4_Validation(EnterIP_Search_TextField.Text)
		  Select Case IPV4_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid IPv4 Address", System.MouseX, System.MouseY, True)
		    #if TargetWin32 Then
		      MsgBox "Please enter a valid IP address"
		    #ENDIF
		    EnterIP_Search_TextField.Text = ""
		    Exit
		  End Select
		  
		  IPInfo_API = New GETIP_Info_HTTP
		  IPInfo_API.SearchIP_Info(EnterIP_Search_TextField.Text)
		  mClearAllFields
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
