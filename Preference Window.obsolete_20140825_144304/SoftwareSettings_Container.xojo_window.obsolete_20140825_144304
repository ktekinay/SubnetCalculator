#tag EndWindow

#tag WindowCode
	#tag Event
		Sub Activate()
		  CurrentAvailVersion_Datalabel.Text = CurrentAvailableVersion
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  // Now Update Preference Window Labels
		  CurrentAvailVersion_Datalabel.Text = CurrentAvailableVersion
		End Sub
	#tag EndEvent

	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = &cFFFFFF
		  g.FillRect(0,0,me.Width,me.Height)
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events UpdateNowButton
	#tag Event
		Sub Action()
		  ManualUpdateStatusBar.Visible = True
		  ManualUpdateStatusBar.Enabled = True
		  Me.Caption = "Checking"
		  Me.Enabled = False
		  
		  VersionCheck.mManuallyLaunchUpdate
		  
		  ManualUpdateStatusBar.Visible = False
		  ManualUpdateStatusBar.Enabled = False
		  Me.Caption = "Check Now"
		  Me.Enabled = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events UpdateSoftwareFreq_PopupMenu
	#tag Event
		Sub Change()
		  If Me.Text = "Every Launch"  Then
		    App.Preferences.CheckForVerUpgradeFreqValue = 0
		    AutoInstall_PopupMenu.Enabled = True
		  Elseif Me.Text = "Weekly"  Then
		    App.Preferences.CheckForVerUpgradeFreqValue = 1
		    AutoInstall_PopupMenu.Enabled = True
		  Elseif Me.Text = "Never" Then
		    App.Preferences.CheckForVerUpgradeFreqValue = 2
		  End If
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop="Every Launch,Weekly,Never"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = App.Preferences.CheckForVerUpgradeFreqValue
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events AutoInstall_PopupMenu
	#tag Event
		Sub Change()
		  If Me.Text = "Auto install"  Then
		    App.Preferences.InstallMethodValue = 0
		    App.Preferences.AutoInstall = True
		  Elseif Me.Text = "Manual install"  Then
		    App.Preferences.InstallMethodValue = 1
		    App.Preferences.AutoInstall = False
		  End If
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  #IF TargetWin32 Then
		    DataPop="Manual install"
		  #ELSEIF TargetMacOS Then
		    DataPop="Auto install,Manual install"
		  #ENDIF
		  
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  #IF TargetWin32 Then
		    me.ListIndex = 0
		  #ELSEIF TargetMacOS THen
		    me.ListIndex = App.Preferences.InstallMethodValue
		  #ENDIF
		End Sub
	#tag EndEvent
#tag EndEvents
