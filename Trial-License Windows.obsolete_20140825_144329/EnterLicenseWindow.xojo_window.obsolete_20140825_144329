#tag WindowCode
#tag EndWindowCode

#tag Events Canvas1
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.PenHeight=1
		  g.PenWidth=1
		  g.ForeColor = &cFFFFFF
		  g.FillRect(0,0,me.Width,me.Height)
		  
		  g.Transparency = 70
		  g.ForeColor = &c000000
		  g.DrawRect(-2,-1,me.Width+4,me.Height)
		  g.Transparency = 0
		  
		  g.DrawPicture(Logosmall,(Self.Width-Logosmall.Width)/2,4)
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseUp(X As Integer, Y As Integer)
		  Dim EmailCMD as New Shell
		  
		  if X >= 216 AND X <=392 AND Y>=268 AND Y<=278 Then
		    #IF TargetMacOS Then
		      EmailCMD.Execute("open ""mailto:support@intelligentvisibility.com?subject=Intelligent Subnet Calculator Question&body=""")
		    #ENDIF
		  End if
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Return True
		End Function
	#tag EndEvent
	#tag Event
		Sub MouseMove(X As Integer, Y As Integer)
		  #IF TargetMacOS OR TargetLinux Then
		    if X >= 216 AND X <=392 AND Y>=268 AND Y<=278 Then
		      Me.MouseCursor = System.Cursors.FingerPointer
		    Else
		      me.MouseCursor = System.Cursors.StandardPointer
		    End if
		  #ENDIF
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events LicenseTextArea
	#tag Event
		Sub TextChange()
		  if Me.Text = "" or UserRegistrationName_Textfield.Text = "" Then
		    ValidateLicense_Button.Enabled = False
		  Else
		    ValidateLicense_Button.Enabled = True
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ValidateLicense_Button
	#tag Event
		Sub Action()
		  If UserRegistrationName_Textfield.Text = "" Then
		    MsgBox "Please enter your full name"
		  End if
		  
		  
		  If LicenseTextArea.Text = "" Then Return
		  Dim AreWePurchased as Boolean = App.Trial_Licensing_Class.fValidation(LicenseTextArea.Text,UserRegistrationName_Textfield.Text)
		  
		  if AreWePurchased = True Then
		    // Show Successful Label
		    LicenseSuccess_Label.Visible = True
		    LicenseFailed_Label.Visible = False
		    
		  Elseif AreWePurchased = False Then
		    // Show failed label
		    LicenseFailed_Label.Visible = True
		    LicenseSuccess_Label.Visible = False
		    LicenseTextArea.Text = ""
		    
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CloseButton
	#tag Event
		Sub Action()
		  EnterLicenseWindow.Close
		End Sub
	#tag EndEvent
#tag EndEvents
