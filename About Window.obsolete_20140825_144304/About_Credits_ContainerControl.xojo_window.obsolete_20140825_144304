#tag WindowCode
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = &cFFFFFF
		  g.FillRect(0,0,me.Width,me.Height)
		  
		  g.Transparency = 70
		  g.ForeColor = &c000000
		  g.DrawRect(-2,-1,me.Width+4,me.Height)
		  g.Transparency = 0
		  
		  g.DrawPicture(Logosmall,(Self.Width-Logosmall.Width)/2,12)
		  
		  g.DrawPicture(IntelligentVisbilitySmall,Me.Width - IntelligentVisbilitySmall.Width-12, me.Height-50)
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events TextArea1
	#tag Event
		Sub Open()
		  
		  Me.StyledText.Text = Strings_Module.kAbout_Credits_LeadDev + EndOfLine +"Mike Cotrone - CCIE #8411 R/S, CCIE #8411 Voice" + EndOfLine + EndOfLine + Strings_Module.kAbout_Credits_SpecialThanks + EndOfLine+_
		  "Tim Hare" + EndOfLine + "Kem Tekinay"+ EndOfLine +"Robert Misior - CCIE #41385 Voice"+EndOfLine + "Scott Boss"
		  
		  // Set all Text Font Type
		  Me.StyledText.Font(0, Len(Me.StyledText.text) ) = "Helvetica"
		  Me.StyledText.Size( 0, Len(Me.StyledText.text) ) = 12
		  
		  // Lead Dev Styles
		  //me.StyledText.Bold(0, 14) = True
		  me.StyledText.Underline(0, 14) = True
		  
		  // Code Contributor Styles
		  //me.StyledText.Bold(29, 36) = True
		  me.StyledText.Underline(64, 15)  = True
		  
		End Sub
	#tag EndEvent
#tag EndEvents
