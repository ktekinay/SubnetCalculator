#tag WindowCode
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = &cFFFFFF
		  g.FillRect(0,0,me.Width,me.Height)
		  
		  g.Transparency = 70
		  g.ForeColor = &c000000
		  g.DrawRect(-2,-1,me.Width+4,me.Height)
		  g.Transparency = 0
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h21
		Private BSD3_EULA_Array() As String
	#tag EndProperty


#tag EndWindowCode

