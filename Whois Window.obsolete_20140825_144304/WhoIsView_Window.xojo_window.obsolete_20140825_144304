		      If AllowLinkBoolean = True Then
		        HTTPCMD.Execute("""C:\Program Files\Internet Explorer\iexplore.exe""" + " " + ARIN_ParentLINK)
		      end if
		    #ENDIF
		  End if
		  
		End Sub
