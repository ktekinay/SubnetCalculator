		  #IF TargetMacOS Then
		    Declare Sub setBezelStyle Lib "Cocoa" Selector "setBezelStyle:" (handle As Integer, value As Integer)
		    setBezelStyle(Me.Handle, 1)
		  #Endif
		End Sub
