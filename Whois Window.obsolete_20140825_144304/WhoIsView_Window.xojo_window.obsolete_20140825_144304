#tag Events ARIN_CIDR_LB
	#tag Event
		Sub Open()
		  // Setup Default Behavior
		  Me.ColumnsResizable = False
		  Me.RequiresSelection = False
		  Me.ColumnCount = 1
		  Me.HasHeading = False
		  Me.AutoHideScrollbars = True
		  Me.ScrollBarHorizontal = False
		  Me.ScrollBarVertical = True
		  Me.UseFocusRing = False
		  //Me.ColumnWidths = "66,112,117,118"
		  Me.TextFont = "Helvetica"
		  Me.TextSize = 12
		  Me.SelectionType = Listbox.SelectionMultiple
		  Me.DefaultRowHeight = 20
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function CellBackgroundPaint(g As Graphics, row As Integer, column As Integer) As Boolean
		  // Get Cell Width
		  Dim RW as integer = me.Column(column).WidthActual
		  Dim RH as integer = me.RowHeight
		  
		  //Fill background Cell color (multicolor) of Only Added Rows
		  If row Mod 2=0 then
		    g.foreColor = App.ThemeColor_Listbox_Color1
		  else
		    g.foreColor = App.ThemeColor_Listbox_Color2
		  end if
		  g.FillRect 0,0,RW,RH
		  
		  if me.selected(Row) Then
		    If row Mod 2=0 then
		      g.foreColor = App.ThemeColor_Listbox_Color1
		    else
		      g.foreColor = App.ThemeColor_Listbox_Color2
		    end if
		    g.FillRect 0,0,RW,RH
		  End if
		  
		  
		  
		  
		  Return True
		  
		  
		  
		End Function
	#tag EndEvent
	#tag Event
		Function CellTextPaint(g As Graphics, row As Integer, column As Integer, x as Integer, y as Integer) As Boolean
		  #pragma Unused x
		  #pragma Unused y
		  
		  // Set Default Font Parms
		  g.TextFont = "System"
		  g.ForeColor = &c000000 // Set Text for all cells to black
		  g.TextSize = 12
		End Function
	#tag EndEvent
	#tag Event
		Function MouseDown(x As Integer, y As Integer) As Boolean
		  
		End Function
	#tag EndEvent
	#tag Event
		Function CompareRows(row1 as Integer, row2 as Integer, column as Integer, ByRef result as Integer) As Boolean
		  If Val(Me.Cell(row1, 0)) < Val(Me.Cell(row2, 0)) Then
		    result = -1
		  ElseIf Val(Me.Cell(row1, 0)) > Val(Me.Cell(row2, 0)) Then
		    result = 1
		  Else
		    result = 0
		  End If
		  
		  Return True
		  
		End Function
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  Return True
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events Separator2
	#tag Event
		Sub Open()
		  #IF TargetMacOS Then
		    Me.Visible = False
		  #ELSEIF TargetWin32 Then
		    Me.Top = -2
		  #ENDIF
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SearchButton
	#tag Event
		Sub Action()
		  If ARIN_LookUpTarget_TextField.text = "" Then Exit
		  Dim NameResolvedToIP as String
		  Dim AlphaOrIP as Boolean = fRecognizeAlpha(ARIN_LookUpTarget_TextField.Text)
		  If AlphaOrIP = True Then
		    NameResolvedToIP = fResolveNameToIP(ARIN_LookUpTarget_TextField.Text)
		  End if
		  Dim ResolvedIP as String = fParseNameResolutionResults(NameResolvedToIP)
		  if ResolvedIP <> "" Then
		    
		    ARIN_Manual_Lookup = New ARIN_HTTP_RestFul
		    ARIN_Manual_Lookup.GetData("ip",ResolvedIP)
		    
		  Elseif ResolvedIP = "" Then
		    
		    Dim IPV4_Validation as String = fIPv4_Validation(ARIN_LookUpTarget_TextField.Text)
		    Select Case IPV4_Validation
		    Case "Not_Valid"
		      Tooltip.Show("Please Enter a Valid IPv4 Address", System.MouseX, System.MouseY, True)
		      ARIN_LookUpTarget_TextField.Text = ""
		      Exit
		    End Select
		    
		    ARIN_Manual_Lookup = New ARIN_HTTP_RestFul
		    ARIN_Manual_Lookup.GetData("ip",ARIN_LookUpTarget_TextField.Text)
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
