#tag EndWindow

#tag WindowCode
	#tag Event
		Sub Close()
		  #IF TargetWin32 Then
		    // If in Trial mode then start Session Count Down Timer on Main Window
		    if App.Trial_Licensing_Class.TrialMode = True Then
		      // Start Countdown Session Timer
		      MainWindow.Trial_CountdownTimer.Period = 1000
		      MainWindow.Trial_CountdownTimer.Mode = Timer.ModeMultiple
		    End if
		  #ENDIF
		End Sub
	#tag EndEvent

	#tag Event
		Function KeyDown(Key As String) As Boolean
		  if Key = Chr(13) Then
		    #IF TargetMacOS Then
		      MainWindow.Left = 100
		      MainWindow.Top =  120
		    #ENDIF
		    
		    If App.TinyViewEnabled = True Then
		      TinyViewWindow.Show
		      StartupWin.Close
		    Elseif app.TinyViewEnabled = False Then
		      MainWindow.Show
		      StartupWin.Close
		    End if
		    
		    
		    
		  End if
		End Function
	#tag EndEvent

	#tag Event
		Sub Open()
		  #IF TargetWin32 Then
		    If App.Trial_Licensing_Class.TrialMode = True Then
		      // Turn On Trial Container
		      TrialVersionContainerControl1.Enabled = True
		      TrialVersionContainerControl1.Visible = True
		      PurchasedVersionContainerControl1.Enabled = False
		      PurchasedVersionContainerControl1.Visible = False
		      
		    Elseif App.Trial_Licensing_Class.TrialMode = False Then
		      // Turn on Purchased Container
		      PurchasedVersionContainerControl1.Enabled = True
		      PurchasedVersionContainerControl1.Visible = True
		      TrialVersionContainerControl1.Enabled = False
		      TrialVersionContainerControl1.Visible = False
		      
		    End If
		  #ENDIF
		End Sub
	#tag EndEvent

	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = RGB(252,249,247)
		  g.FillRect(0,0,me.Width,me.Height)
		  //g.ForeColor = &c000000
		  g.ForeColor = RGB(42,42,42)
		  g.TextSize = 13
		  g.TextFont = "Sabon"
		  
		  #IF TargetMacOS Then
		    g.DrawString(Strings_Module.kAbout_About_Version + app.VersionRelease,484,192)
		  #ELSEIF TargetWin32 Then
		    g.DrawString(Strings_Module.kAbout_About_Version + app.VersionRelease,470,192)
		  #ENDIF
		  
		  g.DrawPicture(Logomedium,me.Width/2 - Logomedium.Width/2,me.Height/2-Logomedium.Height/2-32)
		  
		  #IF TargetMacOS Then
		    g.DrawPicture(IntelligentVisbilitySmall,me.Width-IntelligentVisbilitySmall.Width-8,me.Height-54)
		  #ELSEIF TargetWin32 Then
		    g.DrawPicture(IntelligentVisbilitySmall,me.Width-IntelligentVisbilitySmall.Width-8,me.Height-60)
		  #ENDIF
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h0
		ProgressBarEndValue As Integer = 108
	#tag EndProperty


#tag EndWindowCode

#tag Events ProgressBar1
	#tag Event
		Sub Open()
		  #IF TargetMacOS Then
		    Me.Left = 0
		    Me.Top = 261
		    
		  #ELSEIF TargetWin32 Then
		    Me.Left = -1
		    Me.Top = 263
		    Me.Width = 602
		  #ENDIF
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events StartupTextLabel
	#tag Event
		Sub Open()
		  Me.Text = kAbout_About_CopyRight + Encodings.UTF8.Chr(169) + " 2014 Intelligent Visibility, Inc. - All Rights Reserved"
		  
		  #IF TargetMacOS Then
		    Me.Left = 3
		    Me.Top = 247
		    
		  #ELSEIF TargetWin32 Then
		    Me.Left = 3
		    Me.Top = 242
		  #ENDIF
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events StartUpProgressBarTimer
	#tag Event
		Sub Action()
		  ProgressBar1.Value = ProgressBar1.Value + 24
		  
		  if ProgressBar1.Value > ProgressBarEndValue Then
		    
		    #IF TargetMacOS Then
		      MainWindow.Left = 100
		      MainWindow.Top =  120
		    #ENDIF
		    
		    If App.TinyViewEnabled = True Then
		      TinyViewWindow.Show
		      StartupWin.Close
		      Me.Mode = Timer.ModeOff
		    Elseif app.TinyViewEnabled = False Then
		      MainWindow.Show
		      StartupWin.Close
		      Me.Mode = Timer.ModeOff
		    End if
		    
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
