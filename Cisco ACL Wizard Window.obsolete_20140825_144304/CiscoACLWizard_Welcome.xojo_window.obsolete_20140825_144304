#tag Events Welcome_Textarea
	#tag Event
		Sub Open()
		  Me.StyledText.text = Strings_Module.kACLBuilderMainWelcome_WelcomeMessageLine1 +  EndOfLine + EndOfLine + _
		  Strings_Module.kACLBuilderMainWelcome_WelcomeMessageLine2 + Encodings.UTF8.Chr(174) + Strings_Module.kACLBuilderMainWelcome_WelcomeMessageLine3 + Encodings.UTF8.Chr(174)+ Strings_Module.kACLBuilderMainWelcome_WelcomeMessageLine4
		  Me.StyledText.Font(0, Len(Me.StyledText.text) ) = "Helvetica"
		  Me.StyledText.Size( 0,48) = 16
		  Me.StyledText.Italic(0,48)  = True
		  Me.StyledText.Size(53,80) = 12
		  #IF TargetWin32 Then
		    Me.BackColor = RGB(240,240,240)
		    
		  #ELSEIF TargetMacOS Then
		    Me.BackColor = RGB(232,232,232)
		  #ENDIF
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NetworkImage
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = &cFFFFFF
		  g.FillRect(0,0,me.Width,me.Height)
		  g.DrawPicture(Network244,me.Width/2-Network244.Width/2,me.Height/2-Network244.Height/2)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Action()
		  CiscoACLWizardWindow.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events NextButton
	#tag Event
		Sub Action()
		  CiscoACLWizardWindow.CiscoACLWizard_Welcome1.Visible = False
		  CiscoACLWizardWindow.CiscoACLWizard_Start1.Visible = True
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
