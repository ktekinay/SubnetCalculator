#tag Events DropACLCanvas
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.DrawPicture(DropACLHere,0,0)
		End Sub
	#tag EndEvent
	#tag Event
		Sub DropObject(obj As DragItem, action As Integer)
		  // Only allow .TXT and .RTF Files into drop zone
		  If obj.FolderItem.Type = "TXT Files"  OR obj.FolderItem.Type = "Rich Text Format documents" Then
		    Dim TIS as TextInputStream
		    Dim InFileFolder as FolderItem
		    Dim InFileContents as String
		    TIS = TextInputStream.Open(Obj.FolderItem)
		    InFileContents = TIS.ReadAll
		    
		    // Figure out Which Container to Instantiate the Receive_ACLClass Class on
		    If CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.Visible = True Then
		      CiscoACLWizard_IOS_ExtendedACL1.Receive_ACL_Class  = New Receive_ACLClass
		      ExtendedACLFoundArray() = CiscoACLWizard_IOS_ExtendedACL1.Receive_ACL_Class.fParseReceivedACL(InFileContents,"Extended")
		      
		      if UBound(ExtendedACLFoundArray) = -1 then
		        MsgBox "This is not an Extended IOS/IOS-XE Access List"
		        exit
		      End if
		      
		      // User Validation: Detect if the Dropped ACL Has more than One ACL #
		      If CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CIscoIOS_Extended_ACL_Number.Text = "100" AND CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.DenyPermitPopupMenu.Text = "" Then
		        // Lets Make Sure we only Have One ACL If Not Prompt User on Selection of ACL # To Use
		        MatchedExtACLNumArray() = fCheckForMultipleACLNumbers(ExtendedACLFoundArray)
		        
		        if UBound(MatchedExtACLNumArray) = 0 Then
		          // Only One ACL Number Type Found Move on automatically
		          Redim CiscoACLWizardWindow.IPExtendedACL_Storage_Array(-1)
		          CiscoACLWizard_IOS_ExtendedACL1.mExtendedACL_BulkAdd(ExtendedACLFoundArray,"overwrite")
		        Elseif UBound(MatchedExtACLNumArray) <> 0 Then
		          // WE Found Multiple ACL#'s .. Prompt User to Choose Which One to use for their ACL.
		          mLoadExtACLEntryFoundPopUp
		          MultiExtACLSTDNumWIndow.Show
		        end if
		        
		      end if
		      
		      
		      //////
		    Elseif CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.Visible = True Then
		      CiscoACLWizard_IOS_StandardACL1.Receive_ACL_Class  = New Receive_ACLClass
		      CiscoACLWizardWindow.StandardACLFoundArray()= CiscoACLWizard_IOS_StandardACL1.Receive_ACL_Class.fParseReceivedACL(InFileContents,"Standard")
		      
		      
		      if UBound(StandardACLFoundArray) = -1 then
		        MsgBox "This is not an Standard IOS/IOS-XE Access List"
		        exit
		      Else
		        // User Validation: Detect if the Dropped ACL Has more than One ACL #
		        
		        If CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.CIscoIOS_Standard_ACL_Number.Text = "1" AND CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.DenyPermitPopupMenu.Text = "" Then
		          // Lets Make Sure we only Have One ACL If Not Prompt User on Selection of ACL # To Use
		          MatchedACLNumArray() = fCheckForMultipleACLNumbers(StandardACLFoundArray)
		          
		          if UBound(MatchedACLNumArray) = 0 Then
		            // Only One ACL Number Type Found Move on automatically
		            Redim CiscoACLWizardWindow.IPStandardACL_Storage_Array(-1)
		            CiscoACLWizard_IOS_StandardACL1.mAddEntry_StandardACLBulk(StandardACLFoundArray,"overwrite")
		          Elseif UBound(MatchedACLNumArray) <> 0 Then
		            // WE Found Multiple ACL#'s .. Prompt User to Choose Which One to use for their ACL.
		            mLoadStdACLEntryFoundPopUp
		            MultiACLSTDNumWIndow.Show
		          end if
		          
		          
		        End if
		        
		        
		        
		        
		      End if
		      
		      
		      
		      
		      // Figure out Which Receive_ACLClass to Destroy
		      If CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.Visible = False Then
		        CiscoACLWizard_IOS_ExtendedACL1.Receive_ACL_Class  = Nil
		      Elseif CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.Visible = False Then
		        CiscoACLWizard_IOS_StandardACL1.Receive_ACL_Class  = Nil
		      End if
		      
		    Else
		      if obj.FolderItem.Type = "" Then
		        MsgBox "Sorry this file is not recognized."+EndOfLine+EndOfLine+"Please drop a Text File that contains an ACL"
		      Elseif obj.FolderItem.Type = "Xojo Files" Then
		        MsgBox "Why on earth would you drop a Xojo file on me?"+EndOfLine+EndOfLine+"Please drop a TXT/RTF File that contains an ACL"
		      Else
		        MsgBox "Sorry "+obj.FolderItem.Type + " are not allowed."+EndOfLine+EndOfLine+"Please drop a TXT/RTF File that contains an ACL"
		      End if
		      
		    End if
		    
		  End if
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  SupportedFileTypes_Class = New SupportedFileTypes
		  
		  
		  SupportedFileTypes_Class.mBuildAcceptedFileTypes
		  
		  Dim SupportedfileTypesString as String = SupportedFileTypes_Class.fBuildSupportedfileTypes
		  
		  me.AcceptFileDrop(SupportedfileTypesString)
		End Sub
	#tag EndEvent
#tag EndEvents
