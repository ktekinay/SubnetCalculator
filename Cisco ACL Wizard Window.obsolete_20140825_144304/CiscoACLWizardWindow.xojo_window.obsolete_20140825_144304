#tag WindowCode
	#tag Event
		Sub Close()
		  MainWindow.ACLBuilder.Checked = False
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  me.Title = Strings_Module.kACLBuilderMain_Title
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function fCheckForMultipleACLNumbers(inACLArray() as String) As Integer()
		  Dim LeftArray() as Integer
		  Dim RightArray() as Integer
		  Dim MatchedArray() as Integer
		  Dim x,y,z as Integer
		  
		  for i as integer  = 0 to UBound(inACLArray)
		    // Extract Only the ACL #'s from the Main Array
		    LeftArray.Append CDbl(inACLArray(i).NthField(" ",2))
		    RightArray.Append CDbl(inACLArray(i).NthField(" ",2))
		  next i
		  
		  // Sort Left/Right Arrays
		  LeftArray.Sort
		  RightArray.Sort
		  
		  For x = 0 to UBound(LeftArray)
		    
		    for y = 0 to UBound(RightArray)
		      if LeftArray(x) = RightArray(y) then
		        
		        if UBound(MatchedArray) = -1 then
		          MatchedArray.Append  LeftArray(x)
		        else
		          
		          for z = 0 to UBound(MatchedArray)
		            
		            if LeftArray(x) <> MatchedArray(MatchedArray.Ubound) Then
		              MatchedArray.Append  LeftArray(x)
		              x = x + 1
		              Exit
		            Else
		              x = x + 1
		              Exit
		            End if
		            
		          next z
		        end if
		      end if
		    next y
		  Next x
		  
		  Return MatchedArray
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mLoadExtACLEntryFoundPopUp()
		  // load the Found ACL Entry Array
		  Dim iStr as string
		  for i as integer = 0 to UBound(MatchedExtACLNumArray)
		    iStr = Str(MatchedExtACLNumArray(i))
		    MultiExtACLSTDNumWIndow.ACLNumFoundPopupMenu.AddRow iStr
		  Next i
		  //
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mLoadStdACLEntryFoundPopUp()
		  // load the Found ACL Entry Array
		  Dim iStr as string
		  for i as integer = 0 to UBound(MatchedACLNumArray)
		    iStr = Str(MatchedACLNumArray(i))
		    MultiACLSTDNumWIndow.ACLNumFoundPopupMenu.AddRow iStr
		  Next i
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mMultipleACLNumDialog()
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mResetEverything()
		  // Reset ACL Configuration Container
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.CIscoIOS_Standard_ACL_Number.ListIndex=0
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.DenyPermitPopupMenu.ListIndex = 0
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.LogACLHit_Popupmenu.ListIndex = 0
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.SourceTypePopUpMenu.ListIndex = 0
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.SourceHostIPComboBox.Text = ""
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.SourceWildcardMaskComboBox.Text = ""
		  
		  CiscoACLWizardWindow.IPStandardACL_Storage = Nil
		  ReDim CiscoACLWizardWindow.IPStandardACL_Storage_Array(-1)
		  
		End Sub
	#tag EndMethod


