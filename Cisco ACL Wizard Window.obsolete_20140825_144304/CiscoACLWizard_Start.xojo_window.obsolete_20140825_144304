#tag WindowCode
	#tag Event
		Sub DropObject(obj As DragItem, action As Integer)
		  if Obj.TextAvailable Then
		    
		    MsgBox Obj.Text
		    
		  end if
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  AcceptTextDrop
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h21
		Private ACLTypeDataPop As STring
	#tag EndProperty


#tag EndWindowCode

#tag Events NextButton
	#tag Event
		Sub Action()
		  CiscoACLWizardWindow.CiscoACLWizard_Start1.Visible = False
		  If CIscoAccessListTypePopupMenu.Text = "IP Standard ACL" Then
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.Visible = True
		    
		    // Show DropACL Here Canvas
		    CiscoACLWizardWindow.DropACLCanvas.Visible = True
		    CiscoACLWizardWindow.DropACLCanvas.Left = 10
		    CiscoACLWizardWindow.DropACLCanvas.Top = 190
		    
		  Elseif CIscoAccessListTypePopupMenu.Text = "IP Extended ACL" Then
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.Visible = True
		    CiscoACLWizardWindow.Width = 824
		    CiscoACLWizardWindow.MaxWidth =824
		    CiscoACLWizardWindow.MinWidth = 824
		    
		    CiscoACLWizardWindow.Height = 636
		    CiscoACLWizardWindow.MaxHeight = 636
		    CiscoACLWizardWindow.MinHeight = 636
		    
		    // Show DropACL Here Canvas
		    CiscoACLWizardWindow.DropACLCanvas.Visible =  False
		    CiscoACLWizardWindow.DropACLCanvas.Enabled = False
		    CiscoACLWizardWindow.DropACLCanvas.Left = 10
		    CiscoACLWizardWindow.DropACLCanvas.Top = 490
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Action()
		  CiscoACLWizardWindow.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events validatetitleLabel
	#tag Event
		Sub Open()
		  Me.Text = "Select Operating System and Access List Type"
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CIscoAccessListTypePopupMenu
	#tag Event
		Sub Change()
		  if Me.Text = Strings_Module.kACLBuilderStart_ACLType1 Then
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.CIscoIOS_Standard_ACL_Number.Enabled = True
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.AccessListNumber_Label.Visible = True
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.mCreateStandardACLNumber
		    NextButton.Enabled = True
		    
		  Elseif Me.Text = Strings_Module.kACLBuilderStart_ACLType2 Then
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CIscoIOS_Extended_ACL_Number.Enabled = True
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AccessListNumber_Label.Visible = True
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mCreateExtendedACLNumber
		    NextButton.Enabled = True
		  Elseif Me.Text = "" Then
		    NextButton.Enabled = False
		  End if
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Me.ListIndex = 0
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SelectCiscoOSLabel
	#tag Event
		Sub Open()
		  Me.Text = Strings_Module.kACLBuilderStart_OSType
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CIscoOSTypePopUpMenu
	#tag Event
		Sub Change()
		  if me.Text = "" Then
		    CIscoAccessListTypePopupMenu.Enabled = False
		  Else
		    CIscoAccessListTypePopupMenu.Enabled = True
		  End if
		  
		  Dim ACLTypeDataPop as String
		  Dim ACLTypeLast as Integer
		  
		  // Decide which Selections to Offer for CIscoAccessListTypePopupMenu based on Me.Text selection
		  if me.Text = "Cisco IOS"+Encodings.UTF8.Chr(174)+" / IOS-XE"+Encodings.UTF8.Chr(174) Then
		    ACLTypeDataPop=",IP Standard ACL,IP Extended ACL"//,IP Extended ACL,IP Named ACL,IP Extended Named ACL"
		    ACLTypeLast=CountFields(ACLTypeDataPop,",")
		    
		  Elseif me.Text = "Cisco NXOS" Then
		    ACLTypeDataPop=",IPv4 Access List"
		    ACLTypeLast=CountFields(ACLTypeDataPop,",")
		  End if
		  
		  
		  // Create Menu for Cisco Access List for "CIscoAccessListTypePopupMenu" Popup Menu
		  Dim Last, Counter as Integer
		  
		  Last=CountFields(ACLTypeDataPop,",")
		  CiscoACLWizardWindow.CiscoACLWizard_Start1.CIscoAccessListTypePopupMenu.DeleteAllRows
		  
		  For Counter=1 to last
		    CiscoACLWizardWindow.CiscoACLWizard_Start1.CIscoAccessListTypePopupMenu.addRow NthField(ACLTypeDataPop,",",counter)
		  Next
		  
		  CiscoACLWizardWindow.CiscoACLWizard_Start1.CIscoAccessListTypePopupMenu.ListIndex = 0
		  
		  
		  
		  
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop=" ,"+Strings_Module.kACLBuilderStart_OSTypeOptions+Encodings.UTF8.Chr(174)+Strings_Module.kACLBuilderStart_OSTypeOptions2+Encodings.UTF8.Chr(174)
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
#tag EndEvents
