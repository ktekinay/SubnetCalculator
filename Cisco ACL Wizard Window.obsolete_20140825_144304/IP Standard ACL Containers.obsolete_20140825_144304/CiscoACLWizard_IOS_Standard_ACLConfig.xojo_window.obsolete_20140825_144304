	#tag Event
		Function KeyDown(Key As String) As Boolean
		  // Asc(13) =  Return :: Asc(9) = Tab
		  if Asc(Key) = 13 or Asc(Key) = 9 Then
		    if Me.Text <> "" Then
		      InterfaceNumberComboBoxArray.Append me.Text
		    End If
		  End If
		  
		  Self.SetFocus
		End Function
