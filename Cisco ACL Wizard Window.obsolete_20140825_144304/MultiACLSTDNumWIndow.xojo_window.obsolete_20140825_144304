#tag EndWindow

#tag WindowCode
	#tag Method, Flags = &h0
		Sub mCheckToEnableContinue()
		  if ManualCheckBox.Value = False AND ACLNumFoundPopupMenu.ListIndex = -1 Then
		    ContinueButton.Enabled = False
		    
		  Else
		    ContinueButton.Enabled = True
		  End if
		End Sub
	#tag EndMethod


#tag EndWindowCode

#tag Events Image_Canvas
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.DrawPicture(S_Logo72x72,0,0)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ACLNumFoundPopupMenu
	#tag Event
		Sub Change()
		  mCheckToEnableContinue
		  
		  if me.ListIndex <> -1 Then
		    ManualCheckBox.Enabled = False
		  Else
		    ManualCheckBox.Enabled = True
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ContinueButton
	#tag Event
		Sub Action()
		  // Check to see if the User wants to Manually Choose
		  
		  if ManualCheckBox.Value = True Then
		    // No need to do anything but just close and have the user continue manually
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.mAddEntry_StandardACLBulk(CiscoACLWizardWindow.StandardACLFoundArray,"overwrite")
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.mSet_To_First_Entry
		    MultiACLSTDNumWIndow.Close
		    
		  Elseif ACLNumFoundPopupMenu.ListCount <> -1 Then
		    Redim CiscoACLWizardWindow.IPStandardACL_Storage_Array(-1)
		    CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.mAddEntry_StandardACLBulk(CiscoACLWizardWindow.StandardACLFoundArray,"overwrite",ACLNumFoundPopupMenu.Text)
		    MultiACLSTDNumWIndow.Close
		    
		    
		  End if
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ManualCheckBox
	#tag Event
		Sub Action()
		  mCheckToEnableContinue
		  
		  if me.Value = True Then
		    CiscoACLWizardWindow.UserChoseToManuallyEnter = True
		    ACLNumFoundPopupMenu.Enabled = False
		  Elseif me.Value = False Then
		    ACLNumFoundPopupMenu.Enabled = True
		    CiscoACLWizardWindow.UserChoseToManuallyEnter = False
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
