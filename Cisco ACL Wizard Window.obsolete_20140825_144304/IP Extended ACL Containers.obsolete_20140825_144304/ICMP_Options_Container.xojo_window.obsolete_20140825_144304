	#tag EndEvent
	#tag Event
		Sub Open()
		  me.AddRow ""
		  me.AddRow "administratively-prohibited"
		  me.AddRow "alternate-address"
		  me.AddRow "conversion-error"
		  me.AddRow "dod-host-prohibited"
		  me.AddRow "dod-net-prohibited"
		  me.AddRow "echo"
		  me.AddRow "echo-reply"
		  me.AddRow "general-parameter-problem"
		  me.AddRow "host-isolated"
		  me.AddRow "host-precedence-unreachable"
		  me.AddRow "host-redirect"
		  me.AddRow "host-tos-redirect"
		  me.AddRow "host-tos-unreachable"
		  me.AddRow "host-unknown"
		  me.AddRow "host-unreachable"
		  me.AddRow "information-reply"
		  me.AddRow "information-request"
		  me.AddRow "mask-reply"
		  me.AddRow "mask-redirect"
		  me.AddRow "mobile-redirect"
		  me.AddRow "net-redirect"
		  me.AddRow "net-tos-redirect"
		  me.AddRow "net-tos-unreachable"
		  me.AddRow "net-unreachable"
		  me.AddRow "network-unknown"
		  me.AddRow "no-room-for-option"
		  me.AddRow "option-missing"
		  me.AddRow "packet-too-big"
		  me.AddRow "parameter-problem"
		  me.AddRow "port-unreachable"
		  me.AddRow "precedence-unreachable"
		  me.AddRow "protocol-unreachable"
		  me.AddRow "reassembly-timeout"
		  me.AddRow "redirect"
		  me.AddRow "router-advertisement"
		  me.AddRow "router-solicitation"
		  me.AddRow "source-quench"
		  me.AddRow "source-route-failed"
		  me.AddRow "time-exceeded"
		  me.AddRow "timestamp-reply"
		  me.AddRow "timestamp-request"
		  me.AddRow "traceroute"
		  me.AddRow "ttl-exceeded"
		  me.AddRow "unreachable"
		  
		  for i as integer = 0 to 255
		    me.AddRow Str(i)
		  next i
		  me.ListIndex = 0
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_MessageType = Self.ICMP_MessageType_PopUpMenu.Text
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DSCP_PopUpMenu
	#tag Event
		Sub Change()
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    // Send to Function to Return only Port number
		    Dim QOSValue as String = fConvertToProperQOSValues(Me.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_QOS_DSCP_Value = QOSValue
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events QOS_OptionsPopUpMenu
	#tag Event
		Sub Change()
		  if Me.Text = "" Then
		    TOS_PopUpMenu.Enabled = False
		    TOS_PopUpMenu.ListIndex = 0
		    Precedence_PopUpMenu.Enabled  = False
		    Precedence_PopUpMenu.ListIndex = 0
		    DSCP_PopUpMenu.Enabled = False
		    DSCP_PopUpMenu.ListIndex = 0
		    
		  Elseif Me.Text = "dscp" Then
		    // Hide Other Controls First
		    TOS_PopUpMenu.Enabled = False
		    TOS_PopUpMenu.Visible = False
		    TOS_Choice_Label.Visible = False
		    Precedence_PopUpMenu.Visible = False
		    Precedence_PopUpMenu.Enabled = False
		    Prec_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreate_DSCP_Array
		    DSCP_PopUpMenu.Enabled = True
		    DSCP_PopUpMenu.Visible = True
		    DSCP_Choice_Label.Visible = True
		    
		  Elseif me.Text = "precedence" Then
		    // Hide Other Controls First
		    DSCP_PopUpMenu.Enabled = False
		    DSCP_PopUpMenu.Visible = False
		    DSCP_Choice_Label.Visible = False
		    TOS_PopUpMenu.Enabled = False
		    TOS_PopUpMenu.Visible = False
		    TOS_Choice_Label.Visible = False
		    
		    // Show this Control
		    Precedence_PopUpMenu.Enabled = True
		    Precedence_PopUpMenu.Visible = True
		    Prec_Choice_Label.Visible = True
		    mCreate_Precedence_Array
		    
		  Elseif me.Text = "tos" Then
		    // Hide Other Controls First
		    DSCP_PopUpMenu.Enabled = False
		    DSCP_PopUpMenu.Visible = False
		    DSCP_Choice_Label.Visible = False
		    Precedence_PopUpMenu.Enabled = False
		    Precedence_PopUpMenu.Visible = False
		    Prec_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreate_TOS_Array
		    TOS_PopUpMenu.Enabled = True
		    TOS_PopUpMenu.Visible = True
		    TOS_Choice_Label.Visible = True
		  end if
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.AddRow ""
		  me.AddRow "dscp"
		  me.AddRow "precedence"
		  me.AddRow "tos"
		  me.ListIndex = 0
		  
		  // Hide Other Pop Up Menus
		  TOS_PopUpMenu.Visible = False
		  Precedence_PopUpMenu.Visible = False
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_QOS_Type = Self.QOS_OptionsPopUpMenu.Text
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TOS_PopUpMenu
	#tag Event
		Sub Change()
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mContinueButtonShowOrHide
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    // Send to Function to Return only Port number
		    Dim QOSValue as String = fConvertToProperQOSValues(Me.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_QOS_TOS_Value = QOSValue
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Precedence_PopUpMenu
	#tag Event
		Sub Change()
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    // Send to Function to Return only Port number
		    Dim QOSValue as String = fConvertToProperQOSValues(Me.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_QOS_Prec_Value = QOSValue
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TimeRange_CheckBox
	#tag Event
		Sub Action()
		  if me.Value = True Then
		    // Checked
		    TimeRange_TextField.Enabled = True
		    TimeRange_TextField.SetFocus
		  Elseif me.Value = False Then
		    // Not Checked
		    TimeRange_TextField.Enabled = False
		    
		  End if
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_TimeRange_Set = Self.TimeRange_CheckBox.Value
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TimeRange_TextField
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_TimeRange_Name = Self.TimeRange_TextField.Text
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
