#tag EndEvents
#tag Events UDP_QOS_OptionsPopUpMenu
	#tag Event
		Sub Change()
		  if Me.Text = "" Then
		    UDP_TOS_PopUpMenu.Enabled = False
		    UDP_TOS_PopUpMenu.ListIndex = 0
		    UDP_Precedence_PopUpMenu.Enabled  = False
		    UDP_Precedence_PopUpMenu.ListIndex = 0
		    UDP_DSCP_PopUpMenu.Enabled = False
		    UDP_DSCP_PopUpMenu.ListIndex = 0
		    
		  Elseif Me.Text = "dscp" Then
		    // Hide Other Controls First
		    UDP_TOS_PopUpMenu.Enabled = False
		    UDP_TOS_PopUpMenu.Visible = False
		    TOS_Choice_Label.Visible = False
		    UDP_Precedence_PopUpMenu.Visible = False
		    UDP_Precedence_PopUpMenu.Enabled = False
		    Prec_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreate_DSCP_Array
		    UDP_DSCP_PopUpMenu.Enabled = True
		    UDP_DSCP_PopUpMenu.Visible = True
		    DSCP_Choice_Label.Visible = True
		    
		  Elseif me.Text = "precedence" Then
		    // Hide Other Controls First
		    UDP_DSCP_PopUpMenu.Enabled = False
		    UDP_DSCP_PopUpMenu.Visible = False
		    DSCP_Choice_Label.Visible = False
		    UDP_TOS_PopUpMenu.Enabled = False
		    UDP_TOS_PopUpMenu.Visible = False
		    TOS_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreate_Precedence_Array
		    UDP_Precedence_PopUpMenu.Visible = True
		    UDP_Precedence_PopUpMenu.Enabled = True
		    Prec_Choice_Label.Visible = True
		    
		  Elseif me.Text = "tos" Then
		    // Hide Other Controls First
		    UDP_DSCP_PopUpMenu.Enabled = False
		    UDP_DSCP_PopUpMenu.Visible = False
		    DSCP_Choice_Label.Visible = False
		    UDP_Precedence_PopUpMenu.Visible = False
		    UDP_Precedence_PopUpMenu.Enabled = False
		    Prec_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreate_TOS_Array
		    UDP_TOS_PopUpMenu.Enabled = True
		    UDP_TOS_PopUpMenu.Visible = True
		    TOS_Choice_Label.Visible = True
		  end if
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.AddRow ""
		  me.AddRow "dscp"
		  me.AddRow "precedence"
		  me.AddRow "tos"
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_QOS_Type = Self.UDP_QOS_OptionsPopUpMenu.Text
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
