	#tag Method, Flags = &h0
		Sub mCreateExtendedACLNumber()
		  Dim i,y,xy as Integer
		  for i = 100 to 199
		    CIscoIOS_Extended_ACL_Number.AddRow Str(i)
		  Next i
		  
		  for y = 1300 to 1999
		    CIscoIOS_Extended_ACL_Number.AddRow Str(y)
		  Next y
		  
		  for xy = 2000 to 2699
		    CIscoIOS_Extended_ACL_Number.AddRow Str(xy)
		  Next xy
		  
		  
		  CIscoIOS_Extended_ACL_Number.ListIndex = 0
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub mCreateSourceIPPortArray()
		  SourceIPPortArray.Append ""
		  SourceIPPortArray.Append "tcp"
		  SourceIPPortArray.Append "udp"
		  SourceIPPortArray.Append "ahp"
		  SourceIPPortArray.Append "eigrp"
		  SourceIPPortArray.Append "esp"
		  SourceIPPortArray.Append "gre"
		  SourceIPPortArray.Append "icmp"
		  SourceIPPortArray.Append "igmp"
		  SourceIPPortArray.Append "ip"
		  SourceIPPortArray.Append "ipinip"
		  SourceIPPortArray.Append "ospf"
		  SourceIPPortArray.Append "pim"
		  
		  //<0-255>  An IP protocol number
		  //ahp      Authentication Header Protocol
		  //eigrp    Cisco's EIGRP routing protocol
		  //esp      Encapsulation Security Payload
		  //gre      Cisco's GRE tunneling
		  //icmp     Internet Control Message Protocol
		  //igmp     Internet Gateway Message Protocol
		  //ip       Any Internet Protocol
		  //ipinip   IP in IP tunneling
		  //nos      KA9Q NOS compatible IP over IP tunneling
		  //ospf     OSPF routing protocol
		  //pcp      Payload Compression Protocol
		  //pim      Protocol Independent Multicast
		  //tcp      Transmission Control Protocol
		  //udp      User Datagram Protocol
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub mCreateTCPPortProtocolList(InPopUpMenu as PopupMenu)
		  // If Enteries are already there then don't process
		  Dim AlreadyEntered as Boolean = False
		  for i as integer = 0 to UBound(TCPProtocolPortArray)
		    Select Case TCPProtocolPortArray(i)
		    Case "telnet(23)"
		      AlreadyEntered = True
		      for ii as integer  = 0 to UBound(TCPProtocolPortArray)
		        InPopUpMenu.AddRow TCPProtocolPortArray(ii)
		      next ii
		      exit sub
		    End Select
