		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  // Create Source IP Port Array
		  mCreateSourceIPPortArray
		  
		  // Load Popup Menu from above Array
		  for i as integer = 0 to UBound(SourceIPPortArray)
		    me.addRow SourceIPPortArray(i)
		  Next i
		  
		  
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Global_IP_Protcol_Number = Self.SourceIPProtocolPopUpMenu.Text
		  End if
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SourcePortFilterPopUpMenu
	#tag Event
		Sub Change()
		  if SourceTypePopUpMenu.Text = "Any" and Me.Text = "Range" Then
		    SourceProtocolLabel.Left = 308
		    SourceProtocolLabel.Text = "Starting Port Name / Port"
		    SourceProtocolLabel.Visible = True
		    SourcePortNumberPopUpMenu.Left = 308
		    SourcePortNumberPopUpMenu.Visible = True
		    SourcePortNumberPopUpMenu.Enabled = True
		    
		    
		    SourceRangeEndLabel.Left = 308
		    SourceRangeEndLabel.Top = 209
		    SourceRangeEndLabel.Visible = True
		    SourceRangeEndLabelPopUpMenu.left = 308
		    SourceRangeEndLabelPopUpMenu.top = 228
		    SourceRangeEndLabelPopUpMenu.Visible = True
		    SourceRangeEndLabelPopUpMenu.Enabled  = True
		    
		  Else
		    
		    if me.Text = "Range" Then
		      // Enable Source Controls
		      SourceProtocolLabel.Text = "Starting Port Name / Port"
		      SourceProtocolLabel.Visible = True
		      SourcePortNumberPopUpMenu.Visible = True
		      SourcePortNumberPopUpMenu.Enabled = True
		      
		      SourceRangeEndLabelPopUpMenu.Visible = True
		      SourceRangeEndLabelPopUpMenu.Enabled = True
		      SourceRangeEndLabel.Enabled = True
		      SourceRangeEndLabel.Visible = True
		      
		      
		    Elseif me.Text = "Equal to(eq)" OR  me.Text = "Greater than(gt)" OR  me.Text = "Less than(lt)" OR me.Text = "Not equal to(neq)" Then
		      SourcePortNumberPopUpMenu.Enabled = True
		      SourcePortNumberPopUpMenu.Visible = True
		      SourceProtocolLabel.Text = "Source Port Name / Port"
		      SourceProtocolLabel.Visible = True
		      
		      SourceRangeEndLabelPopUpMenu.Enabled = False
		      SourceRangeEndLabelPopUpMenu.Visible = False
		      SourceRangeEndLabel.Visible = False
		      
		    Elseif Me.Text = "" Then
		      Me.ListIndex = 0
		      SourceProtocolLabel.Text = "Source Port Name / Port"
		      SourceProtocolLabel.Visible = false
		      SourcePortNumberPopUpMenu.Visible = False
		      SourcePortNumberPopUpMenu.Enabled = False
		      SourcePortNumberPopUpMenu.Text = ""
		      SourcePortNumberPopUpMenu.ListIndex = 0
		      
		      SourceRangeEndLabelPopUpMenu.Visible = False
		      SourceRangeEndLabelPopUpMenu.Enabled = False
		      SourceRangeEndLabel.Enabled = False
		      SourceRangeEndLabel.Visible = False
		    End if
		  End if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  
		  // Validations... Ugh
		  If Me.text <> "" AND SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		  End if
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop = ",Equal to(eq),Greater than(gt),Less than(lt),Not equal to(neq),Range"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    
		    // MapValues for Array in Cisco format
		    if me.text = "" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Port_Filter_Operator = ""
		      
		    Elseif me.text = "Equal to(eq)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Port_Filter_Operator = "eq"
		      
		    Elseif me.text = "Greater than(gt)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Port_Filter_Operator = "gt"
		      
		    Elseif me.text = "Less than(lt)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Port_Filter_Operator = "lt"
		      
		    Elseif me.text = "Not equal to(neq)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Port_Filter_Operator = "neq"
		      
		    Elseif me.text = "Range" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Port_Filter_Operator = "range"
		      
		    End if
		    AutoSaved_Canvas.Visible = True
		    
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SourcePortNumberPopUpMenu
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  If key = Chr(9) Or key = Chr(13) Then
		    // Add User entered info to our Array
		    if Me.Text <> "" Then
		      TCPProtocolPortArray.Append me.Text
		    End if
		    
		  End if
		  
		  if Me.Text = "" AND SourcePortFilterPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		  End if
		End Function
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Me.ListIndex = 0
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub Change()
		  if SourceIPProtocolPopUpMenu.Text = "tcp" Then
		    SourcePortNumberPopUpMenu.DeleteAllRows
		    mCreateTCPPortProtocolList(SourcePortNumberPopUpMenu)
		  elseif SourceIPProtocolPopUpMenu.Text = "udp" then
		    SourcePortNumberPopUpMenu.DeleteAllRows
		    mCreateUDPPortProtocolList(SourcePortNumberPopUpMenu)
		  end if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  // Validations Ugh :)
		  if Me.Text = "" AND SourcePortFilterPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		  End if
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  AutoSaved_Canvas.Visible = True
		  
		  if Me.Text = "" AND SourcePortFilterPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		  End if
		  
		  ACL_Config_New_Button.SetFocus
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SourceRangeEndLabelPopUpMenu
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  If key = Chr(9) Or key = Chr(13) Then
		    // Add User entered info to our Array
		    if Me.Text <> "" Then
		      TCPProtocolPortArray.Append me.Text
		    End if
		    
		  End if
		End Function
	#tag EndEvent
	#tag Event
		Sub Change()
		  if SourceIPProtocolPopUpMenu.Text = "tcp" Then
		    SourceRangeEndLabelPopUpMenu.DeleteAllRows
		    mCreateTCPPortProtocolList(SourceRangeEndLabelPopUpMenu)
		  elseif SourceIPProtocolPopUpMenu.Text = "udp" then
		    SourceRangeEndLabelPopUpMenu.DeleteAllRows
		    mCreateUDPPortProtocolList(SourceRangeEndLabelPopUpMenu)
		  end if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  
		End Sub
