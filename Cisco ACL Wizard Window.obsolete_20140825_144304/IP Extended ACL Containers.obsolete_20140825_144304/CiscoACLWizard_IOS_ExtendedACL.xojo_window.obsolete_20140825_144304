	#tag Method, Flags = &h0
		Sub mContinueButtonShowOrHide()
		  // Global Menu Blank Data Validation
		  If DenyPermitPopupMenu.Text = "" Then
		    NextButton.Enabled = False
		  Elseif DenyPermitPopupMenu.Text <> "" AND SourceIPProtocolPopUpMenu.Text = "" AND SourceTypePopUpMenu.Text ="Any" AND DestTypePopUpMenu.Text= "Any" Then
		    NextButton.Enabled = False
		  Elseif DenyPermitPopupMenu.Text <> "" AND SourceIPProtocolPopUpMenu.Text <> "" AND SourceTypePopUpMenu.Text ="Any" AND DestTypePopUpMenu.Text= "Any" Then
		    NextButton.Enabled = True
		  End If
		  
		  // Source Menu Blank Data Validation
		  // Source HOST
		  If SourceTypePopUpMenu.Text = "Host" Then
		    If SourceHostIPComboBox.Text = "" And SourcePortFilterPopUpMenu.Text = "" And SourcePortNumberPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif SourceHostIPComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif SourceHostIPComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text <> "" Then
		      NextButton.Enabled = True
		    Elseif SourceHostIPComboBox.Text = "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif SourceHostIPComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text = "" Then
		      NextButton.Enabled = True
		    End If
		  End If
		  
		  If SourcePortFilterPopUpMenu.Text = "Range" Then
		    If SourcePortNumberPopUpMenu.Text = "" And SourceRangeEndLabelPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif SourcePortNumberPopUpMenu.Text <> "" And SourceRangeEndLabelPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif SourcePortNumberPopUpMenu.Text = "" And SourceRangeEndLabelPopUpMenu.Text <> "" Then
		      NextButton.Enabled = False
		    Elseif SourcePortNumberPopUpMenu.Text <> "" And SourceRangeEndLabelPopUpMenu.Text <> "" Then
		      NextButton.Enabled = True
		    End If
		  End If
		  
		  // SOURCE SUBNET
		  If SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text = "" And SourceWildcardMaskComboBox.Text = "" And SourcePortFilterPopUpMenu.Text = "" And SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text = "" And SourcePortFilterPopUpMenu.Text = "" And SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text = "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text = "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text = "" And SourcePortNumberPopUpMenu.Text = "" Then
		    if SourceTypePopUpMenu.Text = "Host" AND DestHostIPComboBox.Text = "" AND DestPortFilterPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		      
		    Elseif SourceTypePopUpMenu.Text = "Host" AND DestHostIPComboBox.Text = "" AND DestPortFilterPopUpMenu.Text <> "" AND  DestPortNumberPopUpMenu.Text <> "" Then
		      NextButton.Enabled = False
		      
		    Elseif SourceTypePopUpMenu.Text = "Host" AND DestHostIPComboBox.Text <> "" AND DestPortFilterPopUpMenu.Text = "" Then
		      NextButton.Enabled = True
		      
		    Elseif DestTypePopUpMenu.Text = "Subnet" AND DestHostIPComboBox.Text = ""AND DestWildcardMaskComboBox.Text = "" Then
		      NextButton.Enabled = False
		      
		    Elseif DestTypePopUpMenu.Text = "Subnet" AND DestHostIPComboBox.Text <> ""AND DestWildcardMaskComboBox.Text = "" Then
		      NextButton.Enabled = False
		      
		    Elseif DestTypePopUpMenu.Text = "Subnet" AND DestHostIPComboBox.Text = ""AND DestWildcardMaskComboBox.Text <> "" Then
		      NextButton.Enabled = False
		      
		    Elseif DestTypePopUpMenu.Text = "Subnet" AND DestHostIPComboBox.Text <> ""AND DestWildcardMaskComboBox.Text <> "" Then
		      NextButton.Enabled = True
		      
		    Elseif DestTypePopUpMenu.Text = "Any" Then
		      NextButton.Enabled = True
		    End if
		    
		    
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text = "" And SourcePortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text = "" And SourceWildcardMaskComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text = "" And SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text = "" And SourceWildcardMaskComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text = "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif SourceTypePopUpMenu.Text = "Subnet" And SourceHostIPComboBox.Text <> "" And SourceWildcardMaskComboBox.Text <> "" And SourcePortFilterPopUpMenu.Text <> "" And SourcePortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = True
		    
		  End If
		  
		  // Destination Menu Blank Data Validation
		  // DESTINATION HOST
		  If DestTypePopUpMenu.Text = "Host" Then
		    If DestHostIPComboBox.Text = "" And DestPortFilterPopUpMenu.Text = "" And DestPortNumberPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif DestHostIPComboBox.Text <> "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif DestHostIPComboBox.Text <> "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text <> "" Then
		      NextButton.Enabled = True
		    Elseif DestHostIPComboBox.Text = "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif DestHostIPComboBox.Text <> "" And DestPortFilterPopUpMenu.Text = "" Then
		      NextButton.Enabled = True
		    End If
		  End If
		  
		  If DestPortFilterPopUpMenu.Text = "Range" Then
		    If DestPortNumberPopUpMenu.Text = "" And DestRangeEndLabelPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif DestPortNumberPopUpMenu.Text <> "" And DestRangeEndLabelPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif DestPortNumberPopUpMenu.Text = "" And DestRangeEndLabelPopUpMenu.Text <> "" Then
		      NextButton.Enabled = False
		    Elseif DestPortNumberPopUpMenu.Text <> "" And DestRangeEndLabelPopUpMenu.Text <> "" Then
		      NextButton.Enabled = True
		    End If
		  End If
		  
		  // DESTINATION SUBNET
		  If DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text = "" And DestWildcardMaskComboBox.Text = "" And DestPortFilterPopUpMenu.Text = "" And DestPortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text = "" And DestPortFilterPopUpMenu.Text = "" And DestPortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text = "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text = "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text = "" And DestPortNumberPopUpMenu.Text = "" Then
		    if DestTypePopUpMenu.Text = "Host" AND SourceHostIPComboBox.Text = "" AND SourcePortFilterPopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		      
		    Elseif DestTypePopUpMenu.Text = "Host" AND SourceHostIPComboBox.Text = "" AND SourcePortFilterPopUpMenu.Text <> "" AND  SourcePortNumberPopUpMenu.Text <> "" Then
		      NextButton.Enabled = False
		      
		    Elseif DestTypePopUpMenu.Text = "Host" AND SourceHostIPComboBox.Text <> "" AND SourcePortFilterPopUpMenu.Text = "" Then
		      NextButton.Enabled = True
		      
		    Elseif SourceTypePopUpMenu.Text = "Subnet" AND SourceHostIPComboBox.Text = ""AND SourceWildcardMaskComboBox.Text = "" Then
		      NextButton.Enabled = False
		      
		    Elseif SourceTypePopUpMenu.Text = "Subnet" AND SourceHostIPComboBox.Text <> ""AND SourceWildcardMaskComboBox.Text = "" Then
		      NextButton.Enabled = False
		      
		    Elseif SourceTypePopUpMenu.Text = "Subnet" AND SourceHostIPComboBox.Text = ""AND SourceWildcardMaskComboBox.Text <> "" Then
		      NextButton.Enabled = False
		      
		    Elseif SourceTypePopUpMenu.Text = "Subnet" AND SourceHostIPComboBox.Text <> ""AND SourceWildcardMaskComboBox.Text <> "" Then
		      NextButton.Enabled = True
		      
		    Elseif SourceTypePopUpMenu.Text = "Any" Then
		      NextButton.Enabled = True
		    End if
		    
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text = "" And DestPortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text = "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text = "" And DestPortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text = "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text = "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text = "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text = "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = False
		    
		  Elseif DestTypePopUpMenu.Text = "Subnet" And DestHostIPComboBox.Text <> "" And DestWildcardMaskComboBox.Text <> "" And DestPortFilterPopUpMenu.Text <> "" And DestPortNumberPopUpMenu.Text <> "" Then
		    NextButton.Enabled = True
		  End If
		  
		  
		  
		  // CONTAINER VALIDATION
		  
		  // TCP VALIDATION
		  if TCP_Options_Container1.TCP_QOS_OptionsPopUpMenu.Text = "dscp" Then
		    if TCP_Options_Container1.TCP_DSCP_PopUpMenu.Text = "" Then
		      NextButton.Enabled = False
		    Elseif  TCP_Options_Container1.TCP_DSCP_PopUpMenu.Text <> "" Then
		      NextButton.Enabled = True
		    End if
		    
		  Elseif TCP_Options_Container1.TCP_QOS_OptionsPopUpMenu.Text = "tos" Then
		    if TCP_Options_Container1.TCP_TOS_PopUpMenu.Text = "" Then
