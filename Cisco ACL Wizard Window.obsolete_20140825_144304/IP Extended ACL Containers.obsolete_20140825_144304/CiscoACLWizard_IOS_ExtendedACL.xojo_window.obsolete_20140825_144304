	#tag EndEvent
#tag EndEvents
#tag Events DestHostIPComboBox
	#tag Event
		Sub Change()
		  me.DeleteAllRows
		  for i as integer = 0 to UBound(DestWCorLogArray)
		    me.addRow DestWCorLogArray(i)
		  Next i
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  if Me.Text = "" Then
		    NextButton.Enabled = False
		  Else
		    NextButton.Enabled = True
		  End if
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  Tooltip.Hide
		  Select Case key
		  Case Chr(32)
		    Tooltip.Show("Sorry, Spaces not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(33)
		    Tooltip.Show("Sorry, Exclamation mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(34)
		    Tooltip.Show("Sorry, Quotation marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(35)
		    Tooltip.Show("Sorry, Pound sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(36)
		    Tooltip.Show("Sorry, Dollar sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(37)
		    Tooltip.Show("Sorry, Percentage sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(38)
		    Tooltip.Show("Sorry, Ampersand not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(39)
		    Tooltip.Show("Sorry, Apostrophe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(40)
		    Tooltip.Show("Sorry, Open Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(41)
		    Tooltip.Show("Sorry, Closed Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(42)
		    Tooltip.Show("Sorry, Asterisk not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(43)
		    Tooltip.Show("Sorry, Plus signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(44)
		    Tooltip.Show("Sorry, Commas not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(45)
		    Tooltip.Show("Sorry, Hyphen not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 46 (Period)
		    
		  Case Chr(47)
		    Tooltip.Show("Sorry, no Back Slashes allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 48 - 57 (Numbers)
		    
		  Case Chr(58)
		    Tooltip.Show("Sorry, Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(59)
		    Tooltip.Show("Sorry, Semi Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(60)
		    Tooltip.Show("Sorry, Less than bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(61)
		    Tooltip.Show("Sorry, Equal sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(62)
		    Tooltip.Show("Sorry, Greater than bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(63)
		    Tooltip.Show("Sorry, Question Mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(64)
		    Tooltip.Show("Sorry, ""At"" signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    //Case Chr(65) to Chr(90)
		    //Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    //Return True
		    
		  Case Chr(91)
		    Tooltip.Show("Sorry, Open Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(92)
		    Tooltip.Show("Sorry, Back Slash not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(93)
		    Tooltip.Show("Sorry, Closed Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(94)
		    Tooltip.Show("Sorry, Caret not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(95)
		    Tooltip.Show("Sorry, Underscore not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(96)
		    Tooltip.Show("Sorry, Tilde not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    //Case Chr(97) to Chr(122)
		    //Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    //Return True
		    
		  Case Chr(123)
		    Tooltip.Show("Sorry, Open Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(124)
		    Tooltip.Show("Sorry, Pipe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(125)
		    Tooltip.Show("Sorry, Closed Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(126)
		    Tooltip.Show("Sorry, Grave Accent Marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(127)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		  End Select
		  
		  If key = Chr(9) Or key = Chr(13) Then
		    // Only Auto Guess Inverse Mask on Subnet type since Host is always 0.0.0.0
		    if DestTypePopUpMenu.Text = "Subnet" Then
		      Dim GetWildcardMask as String = fSubnet_AutoInverseMask(DestHostIPComboBox)
		      DestWildcardMaskComboBox.Text = GetWildcardMask
		    End if
		    
		    if Me.Text <> "" Then
		      DestWCorLogArray.Append me.Text
		    End if
		    // IP validation
		    Dim IPV4_Validation as String = fIPv4_Validation(me.Text)
		    Select Case IPV4_Validation
		    Case "Not_Valid"
		      Tooltip.Show("Please Enter a Valid IPv4 Address", System.MouseX, System.MouseY, True)
		      Me.Text = ""
		      // Check to see if we Enable or Disable the Continue Button
		      NextButton.Enabled = False
		      Return True
		    Else
		      NextButton.Enabled = True
		    End Select
		  End if
		  
		  
		  
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub MouseMove(X As Integer, Y As Integer)
		  ToolTip.Hide
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseUp(X As Integer, Y As Integer) As Boolean
		  if Me.Text = "" Then
		    Exit
		    
		  Elseif Me.Text <> "" Then
		    DestWCorLogArray.Append me.Text
		  End if
		  
		  // IP validation
		  Dim IPV4_Validation as String = fIPv4_Validation(me.Text)
		  Select Case IPV4_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid Source IP Address", System.MouseX, System.MouseY, True)
		    Me.Text = ""
		    NextButton.Enabled = False
		    Return True
		  Else
		    NextButton.Enabled = True
		  End Select
		End Function
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  if Me.Text <> "" Then
		    DestWCorLogArray.Append me.Text
		  End if
		  // IP validation
		  Dim IPV4_Validation as String = fIPv4_Validation(me.Text)
		  Select Case IPV4_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid Source IP Address", System.MouseX, System.MouseY, True)
		    Me.Text = ""
		    NextButton.Enabled = False
		  Else
		    NextButton.Enabled = True
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Return true
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events DestPortFilterPopUpMenu
	#tag Event
		Sub Change()
		  if DestTypePopUpMenu.Text = "Any" and Me.Text = "Range" Then
		    DestProtocolLabel.Left = 311
		    DestProtocolLabel.Text = "Starting Port Name / Port"
		    DestProtocolLabel.Visible = True
		    DestPortNumberPopUpMenu.Left = 311
		    DestPortNumberPopUpMenu.Enabled = True
		    DestPortNumberPopUpMenu.Visible = True
		    
		    
		    DestRangeEndLabel.Left = 311
		    DestRangeEndLabel.Top = 340
		    DestRangeEndLabel.Visible = True
		    DestRangeEndLabelPopUpMenu.left = 311
		    DestRangeEndLabelPopUpMenu.top = 361
		    DestRangeEndLabelPopUpMenu.Visible = True
		    DestRangeEndLabelPopUpMenu.Enabled  = True
		    
		  Else
		    
		    if me.Text = "Range" Then
		      // Enable Destination Controls
		      DestProtocolLabel.Text = "Starting Port Name / Port"
		      DestProtocolLabel.Visible = True
		      DestPortNumberPopUpMenu.Enabled = True
		      DestPortNumberPopUpMenu.Visible = True
		      
		      DestRangeEndLabelPopUpMenu.Visible = True
		      DestRangeEndLabelPopUpMenu.Enabled = True
		      DestRangeEndLabel.Enabled = True
		      DestRangeEndLabel.Visible = True
		      
		      
		    Elseif me.Text = "Equal to(eq)" OR  me.Text = "Greater than(gt)" OR  me.Text = "Less than(lt)" OR me.Text = "Not equal to(neq)" Then
		      DestPortNumberPopUpMenu.Enabled = True
		      DestPortNumberPopUpMenu.Visible = True
		      DestProtocolLabel.Text = "Destination Port Name / Port"
		      DestProtocolLabel.Visible = True
		      
		      DestRangeEndLabelPopUpMenu.Enabled = False
		      DestRangeEndLabelPopUpMenu.Visible = False
		      DestRangeEndLabel.Visible = False
		      
		    Elseif Me.Text = "" Then
		      Me.ListIndex = 0
		      DestProtocolLabel.Text = "Destination Port Name / Port"
		      DestProtocolLabel.Visible = false
		      DestPortNumberPopUpMenu.Visible = False
		      DestPortNumberPopUpMenu.Enabled = False
		      DestPortNumberPopUpMenu.Text = ""
		      DestPortNumberPopUpMenu.ListIndex = 0
		      
		      DestRangeEndLabelPopUpMenu.Visible = False
		      DestRangeEndLabelPopUpMenu.Enabled = False
		      DestRangeEndLabel.Enabled = False
		      DestRangeEndLabel.Visible = False
		    End if
		  End if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop = ",Equal to(eq),Greater than(gt),Less than(lt),Not equal to(neq),Range"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    // MapValues for Array in Cisco format
		    
		    if me.text = "" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Port_Filter_Operator = ""
		      
		    Elseif me.text = "Equal to(eq)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Port_Filter_Operator  = "eq"
		      
		    Elseif me.text = "Greater than(gt)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Port_Filter_Operator  = "gt"
		      
		    Elseif me.text = "Less than(lt)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Port_Filter_Operator  = "lt"
		      
		    Elseif me.text = "Not equal to(neq)" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Port_Filter_Operator  = "neq"
		      
		    Elseif me.text = "Range" Then
		      CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Port_Filter_Operator  = "range"
		      
		    End if
		    AutoSaved_Canvas.Visible = True
		    
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DestPortNumberPopUpMenu
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  If key = Chr(9) Or key = Chr(13) Then
		    // Add User entered info to our Array
		    if Me.Text <> "" Then
		      TCPProtocolPortArray.Append me.Text
		    End if
		  End if
		End Function
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Me.ListIndex = 0
		End Function
	#tag EndEvent
	#tag Event
		Sub Change()
		  if SourceIPProtocolPopUpMenu.Text = "tcp" Then
		    DestPortNumberPopUpMenu.DeleteAllRows
		    mCreateTCPPortProtocolList(DestPortNumberPopUpMenu)
		  elseif SourceIPProtocolPopUpMenu.Text = "udp" then
		    DestPortNumberPopUpMenu.DeleteAllRows
		    mCreateUDPPortProtocolList(DestPortNumberPopUpMenu)
		  end if
		  
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DestRangeEndLabelPopUpMenu
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  If key = Chr(9) Or key = Chr(13) Then
		    // Add User entered info to our Array
		    if Me.Text <> "" Then
		      TCPProtocolPortArray.Append me.Text
		    End if
		  End if
		End Function
	#tag EndEvent
	#tag Event
		Sub Change()
		  if SourceIPProtocolPopUpMenu.Text = "tcp" Then
		    DestRangeEndLabelPopUpMenu.DeleteAllRows
		    mCreateTCPPortProtocolList(DestRangeEndLabelPopUpMenu)
		  elseif SourceIPProtocolPopUpMenu.Text = "udp" then
		    DestRangeEndLabelPopUpMenu.DeleteAllRows
		    mCreateUDPPortProtocolList(DestRangeEndLabelPopUpMenu)
		  end if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Me.ListIndex = 0
		End Function
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DestTypePopUpMenu
	#tag Event
		Sub Change()
		  if me.Text = "Any" Then
		    // Hide All Dest Menus and labels
		    DestIPHostLabel.Visible = False
		    DestHostIPComboBox.Enabled = False
		    DestHostIPComboBox.Visible = False
		    DestHostIPComboBox.Text = ""
		    DestWildcardMaskLabel.Visible = False
		    DestWildcardMaskComboBox.Enabled = False
		    DestWildcardMaskComboBox.Visible = False
		    DestWildcardMaskComboBox.Text = ""
		    DestProtocolLabel.Visible = False
		    DestPortNumberPopUpMenu.Visible = False
		    DestRangeEndLabel.Visible = False
		    DestRangeEndLabelPopUpMenu.Visible = False
		    DestPortNumberPopUpMenu.Enabled = False
		    
		    //// Newly Position Port Filter, Source Protocol and ENd Range Protocol Controls
		    DestPortFilterLabel.Left = 166
		    DestPortFilterLabel.Top = 296
		    DestPortFilterPopUpMenu.Width = 133
		    DestPortFilterPopUpMenu.Left =  166
		    DestPortFilterPopUpMenu.Top = 316
		    
		    DestProtocolLabel.Left = 308
		    DestProtocolLabel.Top = 296
		    DestPortNumberPopUpMenu.Left = 308
		    DestPortNumberPopUpMenu.Top = 316
		    
		  Elseif me.text = "Host" Then
		    // Show Dest Menus
		    DestHostIPComboBox.Enabled = True
		    DestHostIPComboBox.Visible = True
		    DestIPHostLabel.Visible = True
		    DestWildcardMaskLabel.Visible = False
		    DestWildcardMaskComboBox.Enabled = False
		    DestWildcardMaskComboBox.Visible = False
		    
		    if SourceIPProtocolPopUpMenu.Text = "tcp" OR SourceIPProtocolPopUpMenu.Text = "udp" Then
		      DestPortFilterLabel.Visible = True
		      DestPortFilterPopUpMenu.Visible = True
		      DestPortFilterPopUpMenu.Enabled = True
		      DestProtocolLabel.Visible = True
		      DestPortNumberPopUpMenu.Visible = True
		      if DestPortNumberPopUpMenu.Text = "" Then
		        DestPortNumberPopUpMenu.Enabled = False
		      End if
		    End if
		    
		    //// RE-Position Port Filter, Source Protocol and ENd Range Protocol Controls Back To Default Locations
		    DestPortFilterLabel.Left = 308
		    DestPortFilterLabel.Top = 296
		    DestPortFilterPopUpMenu.Width = 116
		    DestPortFilterPopUpMenu.Left =  308
		    DestPortFilterPopUpMenu.Top = 317
		    
		    
		    DestProtocolLabel.Left = 433
		    DestProtocolLabel.Top = 296
		    DestProtocolLabel.Visible = True
		    DestPortNumberPopUpMenu.Left = 433
		    DestPortNumberPopUpMenu.Top = 317
		    DestPortNumberPopUpMenu.Visible = True
		    DestPortNumberPopUpMenu.Enabled = True
		    DestRangeEndLabel.Left = 433
		    DestRangeEndLabel.Top = 340
		    DestRangeEndLabelPopUpMenu.Left = 433
		    DestRangeEndLabelPopUpMenu.Top = 361
		    DestPortFilterPopUpMenu.ListIndex = 0
		    
		    
		    
		    
		  Elseif me.text = "Subnet" Then
		    // Return Dest Port Filter to regular position
		    Self.DestWildcardMaskComboBox.Text = "" + EndOfLine
		    DestPortFilterLabel.Left = 307
		    DestPortFilterPopUpMenu.Left = 307
		    
		    DestProtocolLabel.Left = 432
		    DestPortNumberPopUpMenu.Left = 432
		    
		    DestRangeEndLabel.Left = DestPortNumberPopUpMenu.Left
		    DestRangeEndLabelPopUpMenu.Left =  DestPortNumberPopUpMenu.Left
		    
		    // Show Dest Menus
		    DestIPHostLabel.Visible = True
		    DestHostIPComboBox.Enabled = True
		    DestHostIPComboBox.Visible = True
		    DestWildcardMaskLabel.Visible = True
		    DestWildcardMaskComboBox.Enabled = True
		    DestWildcardMaskComboBox.Visible = True
		    
		    if SourceIPProtocolPopUpMenu.Text = "tcp" OR SourceIPProtocolPopUpMenu.Text = "udp" Then
		      DestPortFilterLabel.Visible = True
		      DestPortFilterPopUpMenu.Visible = True
		      DestPortFilterPopUpMenu.Enabled = True
		      DestProtocolLabel.Visible = True
		      DestPortNumberPopUpMenu.Visible = True
		      if DestPortNumberPopUpMenu.Text = "" Then
		        DestPortNumberPopUpMenu.Enabled = False
		      End if
		    End if
		    //// RE-Position Port Filter, Source Protocol and ENd Range Protocol Controls Back To Default Locations
		    DestPortFilterLabel.Left = 308
		    DestPortFilterLabel.Top = 296
		    DestPortFilterPopUpMenu.Width = 116
		    DestPortFilterPopUpMenu.Left =  308
		    DestPortFilterPopUpMenu.Top = 317
		    
		    
		    DestProtocolLabel.Left = 433
		    DestProtocolLabel.Top = 296
		    DestProtocolLabel.Visible = True
		    DestPortNumberPopUpMenu.Left = 433
		    DestPortNumberPopUpMenu.Top = 317
		    DestPortNumberPopUpMenu.Visible = True
		    DestPortNumberPopUpMenu.Enabled = True
		    DestRangeEndLabel.Left = 433
		    DestRangeEndLabel.Top = 340
		    DestRangeEndLabelPopUpMenu.Left = 433
		    DestRangeEndLabelPopUpMenu.Top = 361
		    DestPortFilterPopUpMenu.ListIndex = 0
		    
		    
		  end if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop = "Any,Host,Subnet"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Type = Self.DestTypePopUpMenu.Text
		  End if
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DestWildcardMaskComboBox
	#tag Event
		Sub Change()
		  me.DeleteAllRows
		  for i as integer = 0 to UBound(SourceWildcardMaskArray)
		    me.addRow SourceWildcardMaskArray(i)
		  Next i
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  if Me.Text = "" Then
		    NextButton.Enabled = False
		  Else
		    NextButton.Enabled = True
		  End if
		  
		  
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.addRow ""
		  
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  Tooltip.Hide
		  Select Case key
		  Case Chr(32)
		    Tooltip.Show("Sorry, Spaces not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(33)
		    Tooltip.Show("Sorry, Exclamation mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(34)
		    Tooltip.Show("Sorry, Quotation marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(35)
		    Tooltip.Show("Sorry, Pound sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(36)
		    Tooltip.Show("Sorry, Dollar sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(37)
		    Tooltip.Show("Sorry, Percentage sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(38)
		    Tooltip.Show("Sorry, Ampersand not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(39)
		    Tooltip.Show("Sorry, Apostrophe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(40)
		    Tooltip.Show("Sorry, Open Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(41)
		    Tooltip.Show("Sorry, Closed Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(42)
		    Tooltip.Show("Sorry, Asterisk not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(43)
		    Tooltip.Show("Sorry, Plus signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(44)
		    Tooltip.Show("Sorry, Commas not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(45)
		    Tooltip.Show("Sorry, Hyphen not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 46 (Period)
		    
		  Case Chr(47)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 48 - 57 (Numbers)
		    
		  Case Chr(58)
		    Tooltip.Show("Sorry, Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(59)
		    Tooltip.Show("Sorry, Semi Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(60)
		    Tooltip.Show("Sorry, Less than bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(61)
		    Tooltip.Show("Sorry, Equal sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(62)
		    Tooltip.Show("Sorry, Greater than bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(63)
		    Tooltip.Show("Sorry, Question Mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(64)
		    Tooltip.Show("Sorry, ""At"" signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(65) to Chr(90)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(91)
		    Tooltip.Show("Sorry, Open Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(92)
		    Tooltip.Show("Sorry, Back Slash not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(93)
		    Tooltip.Show("Sorry, Closed Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(94)
		    Tooltip.Show("Sorry, Caret not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(95)
		    Tooltip.Show("Sorry, Underscore not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(96)
		    Tooltip.Show("Sorry, Tilde not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(97) to Chr(122)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(123)
		    Tooltip.Show("Sorry, Open Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(124)
		    Tooltip.Show("Sorry, Pipe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(125)
		    Tooltip.Show("Sorry, Closed Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(126)
		    Tooltip.Show("Sorry, Grave Accent Marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(127)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		  End Select
		  
		  If key = Chr(9) Or key = Chr(13) Then
		    if Me.Text <> "" Then
		      SourceWildcardMaskArray.Append me.Text
		    End If
		    
		    // Run Inverse Subnet Mask validation
		    Dim InverseMask_Validation as String = fInverseSubnetMaskValidation(me.Text)
		    Select Case InverseMask_Validation
		    Case "Not_Valid"
		      Tooltip.Show("Please Enter a Valid Inverse Wildcard Mask", System.MouseX, System.MouseY, True)
		      me.Text = ""
		      NextButton.Enabled = False
		      Return True
		    Else
		      me.Text = InverseMask_Validation
		      // Check to see if we Enable or Disable the Continue Button
		      NextButton.Enabled = True
		    End Select
		  End If
		  
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub MouseMove(X As Integer, Y As Integer)
		  Tooltip.Hide
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseUp(X As Integer, Y As Integer) As Boolean
		  // Run Inverse Subnet Mask validation
		  Dim InverseMask_Validation as String = fInverseSubnetMaskValidation(me.Text)
		  Select Case InverseMask_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid Source Inverse Wildcard Mask", System.MouseX, System.MouseY, True)
		    me.Text = ""
		    NextButton.Enabled = False
		    Return True
		  Else
		    me.Text = InverseMask_Validation
		    NextButton.Enabled = True
		  End Select
		  
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  AutoSaved_Canvas.Visible = True
		  
		  // Run Inverse Subnet Mask validation
		  Dim InverseMask_Validation as String = fInverseSubnetMaskValidation(me.Text)
		  Select Case InverseMask_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid Source Inverse Wildcard Mask", System.MouseX, System.MouseY, True)
		    me.Text = ""
		    NextButton.Enabled = False
		  Else
		    me.Text = InverseMask_Validation
		    NextButton.Enabled = True
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Return True
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events ACL_Config_Del_Button
	#tag Event
		Sub Action()
		  mDelEntryFrom_Array_Class (CentralCounter)
		  
		  // If We Delete the Only Entry then replace with a new Entry
		  If UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) = -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage = New IPExtendedACL_Storageclass
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array.Append CiscoACLWizardWindow.IPExtendedACL_Storage
		    //Reset all Options to blank or default selection
		    DenyPermitPopupMenu.ListIndex = 0
		    LogACLHit_Popupmenu.ListIndex = 0
		    SourceTypePopUpMenu.ListIndex = 0
		    SourceHostIPComboBox.Text = ""
		    SourceWildcardMaskComboBox.Text = ""
		  End if
		  
		  // Force Screen To Last Record (Ubound)
		  CentralCounter = UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array)
		  DisplayFirst_DataLabel.Text = Str(CentralCounter+1)
		  // Update UpperBounds Display Counter
		  mSet_Current_DisplayUpperBoundsCounter
		  
		  mLoadEntriesToScreen(CentralCounter)
		  
		  //// Unlock the ACL Number if we are on the First Entry
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) = 0 Then
		    CIscoIOS_Extended_ACL_Number.Enabled = True
		  End if
		  
		  // Unlock The Decision Action
		  DenyPermitPopupMenu.Enabled = True
		  // Shoud we Show the Prev/Next Buttons?
		  mCheckToEnableNextNavButton
		  mCheckToEnablePrevNavButton
		  mCheckToEnableDeleteEntryButton
		  
		  // Show auto "Saved" Message
		  AutoSaved_Canvas.Visible = True
		  
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events LogACLHit_Popupmenu
	#tag Event
		Sub Change()
		  // Disable the Use of the Log-Input if we are "YES"
		  If me.Text = "Yes" Then
		    UDP_Options_Container1.UDP_LogInput_CheckBox.Enabled = False
		    UDP_Options_Container1.UDP_LogInput_CheckBox.Value = False
		    EIGRP_Options_Container1.LogInput_CheckBox.Enabled = False
		    EIGRP_Options_Container1.LogInput_CheckBox.Value = False
		    AHP_Options_Container1.LogInput_CheckBox.Enabled = False
		    AHP_Options_Container1.LogInput_CheckBox.Value = False
		    ESP_Options_Container1.LogInput_CheckBox.Enabled = False
		    ESP_Options_Container1.LogInput_CheckBox.Value = False
		    GRE_Options_Container1.LogInput_CheckBox.Enabled = False
		    GRE_Options_Container1.LogInput_CheckBox.Value = False
		    IP_Options_Container1.LogInput_CheckBox.Enabled = False
		    IP_Options_Container1.LogInput_CheckBox.Value = False
		    IPinIP_Options_Container1.LogInput_CheckBox.Enabled = False
		    IPinIP_Options_Container1.LogInput_CheckBox.Value = False
		    OSPF_Options_Container1.LogInput_CheckBox.Enabled = False
		    OSPF_Options_Container1.LogInput_CheckBox.Value = False
		    IGMP_Options_Container1.LogInput_CheckBox.Enabled = False
		    IGMP_Options_Container1.LogInput_CheckBox.Value = False
		    PIM_Options_Container1.LogInput_CheckBox.Enabled = False
		    PIM_Options_Container1.LogInput_CheckBox.Value = False
		    
		  ElseIf me.Text = "No" Then
		    UDP_Options_Container1.UDP_LogInput_CheckBox.Enabled = True
		    EIGRP_Options_Container1.LogInput_CheckBox.Enabled = True
		    AHP_Options_Container1.LogInput_CheckBox.Enabled = True
		    ESP_Options_Container1.LogInput_CheckBox.Enabled = True
		    GRE_Options_Container1.LogInput_CheckBox.Enabled = True
		    IP_Options_Container1.LogInput_CheckBox.Enabled = True
		    IPinIP_Options_Container1.LogInput_CheckBox.Enabled = True
		    OSPF_Options_Container1.LogInput_CheckBox.Enabled = True
		    IGMP_Options_Container1.LogInput_CheckBox.Enabled = True
		    PIM_Options_Container1.LogInput_CheckBox.Enabled = True
		  End if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop = "No,Yes"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Global_Log_Match = Self.LogACLHit_Popupmenu.Text
		  End if
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
