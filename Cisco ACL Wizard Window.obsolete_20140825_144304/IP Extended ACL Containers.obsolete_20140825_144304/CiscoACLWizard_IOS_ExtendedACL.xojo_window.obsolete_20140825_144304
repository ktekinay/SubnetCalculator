	#tag Method, Flags = &h0
		Sub mDelEntryFrom_Array_Class(ACLStorageClassArrayIndex as Integer)
		  CiscoACLWizardWindow.IPExtendedACL_Storage_Array.Remove (ACLStorageClassArrayIndex)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mExtendedACL_BulkAdd(inExtendedACL_Array() as String, optional inOverWriteCC as String, optional inACLNumToUse as String)
		  if inOverWriteCC = "overwrite" Then
		    CentralCounter = 0
		  Else
		    CentralCounter =CentralCounter + 1
		  End if
		  
		  ////////////// DEFINE VARIABLES
		  For i As Integer = UBound(inExtendedACL_Array) DownTo 0
		    Dim Global_ACL_Number, Global_ACL_Decision, Global_Log_Match As String
		    Dim Source_Type, Source_Address, SourceWildCardMask, SourceIPProtocol as String
		    Dim Dest_Type, Dest_Address, DestWildCardMask, DestIPProtocol as String
		    
		    Dim FourthField, FifthField, SixthField,SeventhField,EigthField,NinthField,TenthField,EleventhField,TwelveField,ThirteenthField As String
		    Dim Fourteenth, FifteenthField, SixteenthField, SeventeenthField, EighteenthField as String
		    
		    // Probably need to measure to see how long our ACL is so we know HOW many Fields to Apply
		    // Assign Each Field into the corresponding Variable
		    Global_ACL_Number = Trim(inExtendedACL_Array(i).NthField(" ",2))
		    Global_ACL_Decision = Trim(inExtendedACL_Array(i).NthField(" ",3))
		    FourthField = Trim(inExtendedACL_Array(i).NthField(" ",4)) // IP Protocol
		    FifthField = Trim(inExtendedACL_Array(i).NthField(" ",5))
		    SixthField = Trim(inExtendedACL_Array(i).NthField(" ",6))
		    SeventhField = Trim(inExtendedACL_Array(i).NthField(" ",7))
		    EigthField = Trim(inExtendedACL_Array(i).NthField(" ",8))
		    NinthField = Trim(inExtendedACL_Array(i).NthField(" ",9))
		    TenthField = Trim(inExtendedACL_Array(i).NthField(" ",10))
		    EleventhField = Trim(inExtendedACL_Array(i).NthField(" ",11))
		    TwelveField = Trim(inExtendedACL_Array(i).NthField(" ",12))
		    ThirteenthField = Trim(inExtendedACL_Array(i).NthField(" ",13))
		    Fourteenth = Trim(inExtendedACL_Array(i).NthField(" ",14))
		    FifteenthField = Trim(inExtendedACL_Array(i).NthField(" ",15))
		    SixteenthField = Trim(inExtendedACL_Array(i).NthField(" ",16))
		    SeventeenthField = Trim(inExtendedACL_Array(i).NthField(" ",17))
		    EighteenthField = Trim(inExtendedACL_Array(i).NthField(" ",18))
		    
		    // access-list 100 permit tcp host 10.1.1.1 eq 531 any
		    // access-list 100 deny udp any host 10.1.1.1 eq 9 log
		    
		    Select Case FourthField
		    Case "ip" // IP PROTOCOL
		      
		      SourceIPProtocol = FourthField
		      
		      //                                5     6       7      8                9
		      // access-list 100 permit ip   any   any dscp               0
		      // access-list 100 permit ip   any   any log-input
		      // access-list 100 permit ip   any   any fragments
		      // access-list 100 permit ip   any   any precedence    3
		      // access-list 100 deny    ip   any   any time-range    timllo
		      
		      //                                            5       6         7               8             9       10
		      // access-list 100 permit tcp host 10.1.1.1   eq          531         any
		      // access-list 100 deny   udp any   host     10.1.1.1     eq           9        log
		      
		      
		      if FifthField = "host" Then // SRC
		        Source_Type = "Host"
		        Source_Address = SixthField
		        if SeventhField = "any"  Then // DST
		          Dest_Type = "Any"
		          if EigthField = "log" Then
		            Global_Log_Match = "Yes"
		          end if
		        Elseif SeventhField = "host" then // DST
		          Dest_Type = "Host"
		          Dest_Address = EigthField
		          if NinthField = "log" Then
		            Global_Log_Match = "Yes"
		          end if
		          
		        Elseif SeventhField = "eq" then
		          
		        Else // // DST Subnet Network
		          Dest_Type = "Subnet"
		          Dest_Address= SeventhField
		          DestWildCardMask = EigthField
		          if NinthField = "log" Then
		            Global_Log_Match = "Yes"
		          end if
		        end if
		        
		      Elseif FifthField = "any" Then // SRC
		        Source_Type = "Any"
		        
		        if SixthField = "host" then
		          Dest_Type = "Host"
		          Dest_Address = SeventhField
		          DestWildCardMask = EigthField
		          if NinthField = "log" Then
		            Global_Log_Match = "Yes"
		          end if
		        Else
		          Dest_Type = "Subnet"
		          Dest_Address = SixthField
		          DestWildCardMask = SeventhField
		          if EigthField= "log" Then
		            Global_Log_Match = "Yes"
		          end if
		        end if
		        
		      end if
		      
		      
		      
		      
		      
		      
		      
		      
		    Case "tcp"
		      Source_Type = FourthField
		    Case "udp"
		      Source_Type = FourthField
		    Case "icmp"
		      Source_Type = FourthField
		    Case "eigrp"
		      Source_Type = FourthField
		    Case "ahp"
		      Source_Type = FourthField
		    Case "esp"
		      Source_Type = FourthField
		    Case "gre"
		      Source_Type = FourthField
		    Case "ipnip"
		      Source_Type = FourthField
		    Case "ospf"
		      Source_Type = FourthField
		    Case "pim"
		      Source_Type = FourthField
		    Case "igmp"
		      Source_Type = FourthField
		    Else // IP Address Subnets
		      Source_Address = FourthField
		    End Select
		    
		    
		    
		    
		    ////////////// END OF LOGIC
		    
		    //// Re-Instantiate New Class and Create New Class Array Enty
		    CiscoACLWizardWindow.IPExtendedACL_Storage = New IPExtendedACL_StorageClass
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array.Insert CentralCounter,CiscoACLWizardWindow.IPExtendedACL_Storage
		    
		    //Save One Array Element Worth of ACL Entries into the Class Just Created
		    if inACLNumToUse = "" Then
		      mSaveBulkEntriesToClass(Global_ACL_Number,Global_ACL_Decision,SourceIPProtocol, Source_Type,Source_Address,SourceWildCardMask, Dest_Type,Dest_Address,DestWildCardMask,Global_Log_Match)
		    Elseif inACLNumToUse <> "" Then
		      //mSaveBulkEntriesToClass(inExtendedACL_Array,Global_ACL_Decision,Source_Type,Source_Address.....,Global_Log_Match)
		    End if
		    
		    // Disable the Combo Boxes
		    SourceHostIPComboBox.Enabled = False
		    SourceWildcardMaskComboBox.Enabled = False
		  Next i
		  
		  CentralCounter = UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array)
		  mLoadEntriesToScreen(CentralCounter)
		  DisplayFirst_DataLabel.Text = Str(CentralCounter+1)
		  // Update UpperBounds Display Counter
		  mSet_Current_DisplayUpperBoundsCounter
		  // Shoud we Show the Prev/Next Buttons?
		  mCheckToEnableNextNavButton
		  mCheckToEnablePrevNavButton
		  
		  
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mLoadEntriesToScreen(ACLStorageClassArrayIndex as Integer)
		  // Load Every Source Entry (Hidden or Not)
		  
		  //Load Global Entries
		  for i as integer = 0 to CIscoIOS_Extended_ACL_Number.ListCount-1
		    if CIscoIOS_Extended_ACL_Number.List(0)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Global_ACL_Number  Then
		      CIscoIOS_Extended_ACL_Number.ListIndex = 1
		    End if
		  Next i
		  
		  for i as integer = 0 to DenyPermitPopupMenu.ListCount-1
		    if DenyPermitPopupMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Global_ACL_Decision  Then
		      DenyPermitPopupMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to SourceIPProtocolPopUpMenu.ListCount-1
		    if SourceIPProtocolPopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Global_IP_Protcol_Number  Then
		      SourceIPProtocolPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to LogACLHit_Popupmenu.ListCount-1
		    if LogACLHit_Popupmenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Global_Log_Match  Then
		      LogACLHit_Popupmenu.ListIndex = i
		    End if
		  Next i
		  
		  // Load Source Entries
		  for i as integer = 0 to SourceTypePopUpMenu.ListCount-1
		    if SourceTypePopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Source_Type  Then
		      SourceTypePopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  SourceHostIPComboBox.Text  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Source_Address
		  SourceWildcardMaskComboBox.Text  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Source_WildcardMask
		  
		  for i as integer = 0 to SourcePortFilterPopUpMenu.ListCount-1
		    Dim RemappedPortFilterValue as String = fReMap_IOS_PFVals_ToUserVals(SourcePortFilterPopUpMenu.List(i))
		    
		    if RemappedPortFilterValue  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Source_Port_Filter_Operator  Then
		      SourcePortFilterPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  
		  SourcePortNumberPopUpMenu.Text  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Source_Range_Start_Port
		  SourceRangeEndLabelPopUpMenu.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Source_Range_End_Port
		  
		  // Load Destination Entries
		  for i as integer = 0 to DestTypePopUpMenu.ListCount-1
		    if DestTypePopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Destination_Type  Then
		      DestTypePopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  DestHostIPComboBox.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Destination_Address
		  DestWildcardMaskComboBox.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Destination_WildcardMask
		  
		  for i as integer = 0 to DestPortFilterPopUpMenu.ListCount-1
		    Dim RemappedDestPortFilterValue as String = fReMap_IOS_PFVals_ToUserVals(DestPortFilterPopUpMenu.List(i))
		    if RemappedDestPortFilterValue  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Destination_Port_Filter_Operator  Then
		      DestPortFilterPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  DestPortNumberPopUpMenu.Text= CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Destination_Range_Start_Port
		  DestRangeEndLabelPopUpMenu.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).Destination_Range_End_Port
		  
		  ////////////////////////////
		  //  TCP Options //
		  //////////////////////////
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_QOS_OptionsPopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_QOS_OptionsPopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_QOS_Type  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_QOS_OptionsPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_DSCP_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_DSCP_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_QOS_DSCP_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_DSCP_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_TOS_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_TOS_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_QOS_TOS_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_TOS_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_Precedence_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_Precedence_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_QOS_Prec_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.TCP_Options_Container1.TCP_Precedence_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  TCP_Options_Container1.TCP_Established_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Established
		  TCP_Options_Container1.TCP_Syn_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Syn_Set
		  TCP_Options_Container1.TCP_Push_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Push_Set
		  TCP_Options_Container1.TCP_Reset_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Reset_Set
		  TCP_Options_Container1.TCP_Ack_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Ack_Set
		  TCP_Options_Container1.TCP_Fragments_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Fragments_Set
		  TCP_Options_Container1.TCP_Fin_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Fin_Set
		  TCP_Options_Container1.TCP_Urgent_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Urgent_Set
		  TCP_Options_Container1.TimeRange_TextField.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_TimeRange_Name
		  TCP_Options_Container1.TimeRange_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_TimeRange_Set
		  
		  ////////////////////////////
		  //  UDP Options //
		  //////////////////////////
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_QOS_OptionsPopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_QOS_OptionsPopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_QOS_Type  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_QOS_OptionsPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_DSCP_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_DSCP_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_QOS_DSCP_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_DSCP_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_TOS_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_TOS_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_QOS_TOS_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_TOS_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_Precedence_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_Precedence_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_QOS_Prec_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.UDP_Options_Container1.UDP_Precedence_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  UDP_Options_Container1.UDP_Fragments_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_Checkmark_Fragments_Set
		  UDP_Options_Container1.UDP_LogInput_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_Checkmark_LogInput_Set
		  UDP_Options_Container1.TimeRange_TextField.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_TimeRange_Name
		  UDP_Options_Container1.TimeRange_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_UDP_TimeRange_Set
		  
		  ////////////////////////////////
		  //  EIGRP Options //
		  //////////////////////////////
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.QOS_OptionsPopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.QOS_OptionsPopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_QOS_Type  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.QOS_OptionsPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.DSCP_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.DSCP_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_QOS_DSCP_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.DSCP_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.TOS_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.TOS_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_QOS_TOS_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.TOS_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.Precedence_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.Precedence_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_QOS_Prec_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.EIGRP_Options_Container1.Precedence_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  EIGRP_Options_Container1.Fragments_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_Checkmark_Fragments_Set
		  EIGRP_Options_Container1.LogInput_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_Checkmark_LogInput_Set
		  EIGRP_Options_Container1.TimeRange_TextField.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_TimeRange_Name
		  EIGRP_Options_Container1.TimeRange_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_EIGRP_TimeRange_Set
		  
		  
		  /////////////////////////////
		  //  AHP Options //
		  //////////////////////////
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AHP_Options_Container1.QOS_OptionsPopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AHP_Options_Container1.QOS_OptionsPopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_AHP_QOS_Type  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AHP_Options_Container1.QOS_OptionsPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AHP_Options_Container1.DSCP_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AHP_Options_Container1.DSCP_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_AHP_QOS_DSCP_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AHP_Options_Container1.DSCP_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
