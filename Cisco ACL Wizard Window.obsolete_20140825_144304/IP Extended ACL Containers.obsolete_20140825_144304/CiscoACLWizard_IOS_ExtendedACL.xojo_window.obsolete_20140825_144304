		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.ICMP_Options_Container1.Precedence_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.ICMP_Options_Container1.Precedence_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_QOS_Prec_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.ICMP_Options_Container1.Precedence_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.ICMP_Options_Container1.ICMP_MessageType_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.ICMP_Options_Container1.ICMP_MessageType_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_MessageType  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.ICMP_Options_Container1.ICMP_MessageType_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  ICMP_Options_Container1.TimeRange_TextField.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_TimeRange_Name
		  ICMP_Options_Container1.TimeRange_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_ICMP_TimeRange_Set
		  
		  //////////////////////////////
		  //  IGMP Options //
		  ////////////////////////////
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.QOS_OptionsPopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.QOS_OptionsPopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_QOS_Type  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.QOS_OptionsPopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.DSCP_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.DSCP_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_QOS_DSCP_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.DSCP_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.TOS_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.TOS_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_QOS_TOS_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.TOS_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.Precedence_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.Precedence_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_QOS_Prec_Value  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.Precedence_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  for i as integer = 0 to CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.IGMP_MessageType_PopUpMenu.ListCount-1
		    if CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.IGMP_MessageType_PopUpMenu.List(i)  = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_MessageType  Then
		      CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.IGMP_Options_Container1.IGMP_MessageType_PopUpMenu.ListIndex = i
		    End if
		  Next i
		  
		  IGMP_Options_Container1.Fragments_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_Checkmark_Fragments_Set
		  IGMP_Options_Container1.LogInput_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_Checkmark_LogInput_Set
		  IGMP_Options_Container1.TimeRange_TextField.Text = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_TimeRange_Name
		  IGMP_Options_Container1.TimeRange_CheckBox.Value = CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_IGMP_TimeRange_Set
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mPrepareFirstEntry()
		  //// Create First Empty Index
		  CiscoACLWizardWindow.IPExtendedACL_Storage = New IPExtendedACL_Storageclass
		  CiscoACLWizardWindow.IPExtendedACL_Storage_Array.Append CiscoACLWizardWindow.IPExtendedACL_Storage
		  
		  CentralCounter = 0
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mSaveBulkEntriesToClass(inGlobal_ACL_Number as String, inGlobal_ACL_Decision as String, inGlobalIPProtocol as String, inSource_Type as String, inSource_Address as String, inSourceWildCardMask as String, inDest_Type as String, inDest_Address as String, inDestWildCardMask as String, inGlobal_Log_Match as String)
		  ////// NEED TO BE EXPANDED BASED ON EXT ACL OPTIONS
		  // Globals
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_ACL_Number =  inGlobal_ACL_Number
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_ACL_Decision =  inGlobal_ACL_Decision
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_IP_Protcol_Number = inGlobalIPProtocol
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_Log_Match = inGlobal_Log_Match
		  
		  // Source
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Source_Type = inSource_Type
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Source_Address = inSource_Address
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Source_WildcardMask = inSourceWildCardMask
		  
		  // Dest
		  
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Destination_Type = inDest_Type
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Destination_Address = inDest_Address
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Destination_WildcardMask = inDestWildCardMask
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub mSaveComboBoxWorkAround()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    
		    // Source Start and End Ports
		    Dim SourceStartPortNumber as String = fConvertToPortNumbers(SourcePortNumberPopUpMenu.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Range_Start_Port = SourceStartPortNumber
		    Dim SourceEndPortNumber as String = fConvertToPortNumbers(SourceRangeEndLabelPopUpMenu.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Range_End_Port = SourceEndPortNumber
		    
		    // Destination Start and End Ports
		    Dim DestStartPortNumber as String = fConvertToPortNumbers(DestPortNumberPopUpMenu.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Range_Start_Port = DestStartPortNumber
		    
		    Dim DestEndPortNumber as String = fConvertToPortNumbers(DestRangeEndLabelPopUpMenu.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Range_End_Port = DestEndPortNumber
		    
		    // Source IP Address/Mask
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Address= SourceHostIPComboBox.Text
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_WildcardMask= SourceWildcardMaskComboBox.Text
		    
		    // Dest IP Address / Mask
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_Address = DestHostIPComboBox.Text
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Destination_WildcardMask = DestWildcardMaskComboBox.Text
		    
		    
		  End if
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mSaveEntriesToClass()
		  // Save Every Source Entry (Hidden or Not)
		  
		  // Save Global Entries
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_ACL_Number = CIscoIOS_Extended_ACL_Number.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_ACL_Decision = DenyPermitPopupMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_IP_Protcol_Number = SourceIPProtocolPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Global_Log_Match = LogACLHit_Popupmenu.Text
		  
		  // Save Source Entries
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Source_Type = SourceTypePopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Source_Address = SourceHostIPComboBox.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Source_WildcardMask = SourceWildcardMaskComboBox.Text
		  
		  // Save Destination Entries
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Destination_Type = DestTypePopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Destination_Address =DestHostIPComboBox.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.Destination_WildcardMask = DestWildcardMaskComboBox.Text
		  
		  //////////////////
		  //  TCP Options //
		  ////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_QOS_Type = TCP_Options_Container1.TCP_QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Established =   TCP_Options_Container1.TCP_Established_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Syn_Set = TCP_Options_Container1.TCP_Syn_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Push_Set = TCP_Options_Container1.TCP_Push_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Reset_Set = TCP_Options_Container1.TCP_Reset_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Ack_Set = TCP_Options_Container1.TCP_Ack_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Fragments_Set = TCP_Options_Container1.TCP_Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Fin_Set = TCP_Options_Container1.TCP_Fin_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_Checkmark_Urgent_Set = TCP_Options_Container1.TCP_Urgent_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_TimeRange_Name = TCP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_TCP_TimeRange_Set = TCP_Options_Container1.TimeRange_CheckBox.Value
		  //////////////////
		  //  UDP Options //
		  /////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_UDP_QOS_Type = UDP_Options_Container1.UDP_QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_UDP_Checkmark_Fragments_Set = UDP_Options_Container1.UDP_Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_UDP_Checkmark_LogInput_Set = UDP_Options_Container1.UDP_LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_UDP_TimeRange_Name = UDP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_UDP_TimeRange_Set = UDP_Options_Container1.TimeRange_CheckBox.Value
		  ////////////////////
		  //  EIGRP Options //
		  //////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_EIGRP_QOS_Type = EIGRP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_EIGRP_Checkmark_Fragments_Set = EIGRP_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_EIGRP_Checkmark_LogInput_Set = EIGRP_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_EIGRP_TimeRange_Name = EIGRP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_EIGRP_TimeRange_Set = EIGRP_Options_Container1.TimeRange_CheckBox.Value
		  /////////////////////////////
		  //  AHP Options //
		  //////////////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_AHP_QOS_Type = AHP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_AHP_Checkmark_Fragments_Set = AHP_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_AHP_Checkmark_LogInput_Set = AHP_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_AHP_TimeRange_Name = AHP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_AHP_TimeRange_Set = AHP_Options_Container1.TimeRange_CheckBox.Value
		  //////////////////
		  //  ESP Options //
		  ////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ESP_QOS_Type = ESP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ESP_Checkmark_Fragments_Set = ESP_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ESP_Checkmark_LogInput_Set = ESP_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ESP_TimeRange_Name = ESP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ESP_TimeRange_Set = ESP_Options_Container1.TimeRange_CheckBox.Value
		  //////////////////
		  //  GRE Options //
		  ////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_GRE_QOS_Type = GRE_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_GRE_Checkmark_Fragments_Set = GRE_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_GRE_Checkmark_LogInput_Set = GRE_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_GRE_TimeRange_Name = GRE_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_GRE_TimeRange_Set = GRE_Options_Container1.TimeRange_CheckBox.Value
		  ////////////////////
		  //  IPinIP Options //
		  //////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IPinIP_QOS_Type = IPinIP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IPinIP_Checkmark_Fragments_Set = IPinIP_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IPinIP_Checkmark_LogInput_Set = IPinIP_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IPinIP_TimeRange_Name = IPinIP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IPinIP_TimeRange_Set = IPinIP_Options_Container1.TimeRange_CheckBox.Value
		  ////////////////
		  //  IP Options //
		  //////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IP_QOS_Type = IP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IP_Checkmark_Fragments_Set = IP_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IP_Checkmark_LogInput_Set = IP_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IP_TimeRange_Name = IP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IP_TimeRange_Set = IP_Options_Container1.TimeRange_CheckBox.Value
		  ////////////////////
		  //  OSPF Options //
		  /////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_OSPF_QOS_Type = OSPF_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_OSPF_Checkmark_Fragments_Set = OSPF_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_OSPF_Checkmark_LogInput_Set = OSPF_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_OSPF_TimeRange_Name = OSPF_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_OSPF_TimeRange_Set = OSPF_Options_Container1.TimeRange_CheckBox.Value
		  //////////////////
		  //  PIM Options //
		  ////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_PIM_QOS_Type = PIM_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_PIM_Checkmark_Fragments_Set = PIM_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_PIM_Checkmark_LogInput_Set = PIM_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_PIM_TimeRange_Name = PIM_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_PIM_TimeRange_Set = PIM_Options_Container1.TimeRange_CheckBox.Value
		  ///////////////////
		  //  ICMP Options //
		  /////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ICMP_QOS_Type = ICMP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ICMP_MessageType = ICMP_Options_Container1.ICMP_MessageType_PopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ICMP_TimeRange_Name = ICMP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_ICMP_TimeRange_Set = ICMP_Options_Container1.TimeRange_CheckBox.Value
		  ///////////////////
		  //  IGMP Options //
		  //////////////////
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IGMP_QOS_Type = IGMP_Options_Container1.QOS_OptionsPopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IGMP_MessageType = IGMP_Options_Container1.IGMP_MessageType_PopUpMenu.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IGMP_Checkmark_Fragments_Set = IGMP_Options_Container1.Fragments_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IGMP_Checkmark_LogInput_Set = IGMP_Options_Container1.LogInput_CheckBox.Value
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IGMP_TimeRange_Name = IGMP_Options_Container1.TimeRange_TextField.Text
		  CiscoACLWizardWindow.IPExtendedACL_Storage.OPTIONS_IGMP_TimeRange_Set = IGMP_Options_Container1.TimeRange_CheckBox.Value
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mSet_Current_DisplayCounter()
		  DisplayFirst_DataLabel.Text = Str(CentralCounter+1)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mSet_Current_DisplayUpperBoundsCounter()
		  DisplayUbound_DataLabel.Text = Str(UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array)+1)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub mSet_To_First_Entry()
		  mClearControls
		  CentralCounter = 0
		  // Load Values to Display
		  mLoadEntriesToScreen(CentralCounter)
		  
		  //// Unlock the ACL Number if we are on the First Entry
		  if CentralCounter = 0 Then
		    CIscoIOS_Extended_ACL_Number.Enabled = True
		  End if
		  
		  // Update Display Stats on ACL Setting Screen
		  mSet_Current_DisplayCounter
		  
		  // Shoud we Show the Prev/Next Buttons?
		  mCheckToEnableNextNavButton
		  mCheckToEnablePrevNavButton
		  //
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		CentralCounter As INteger = 0
	#tag EndProperty

	#tag Property, Flags = &h21
		Private DestWCorLogArray() As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Options_DSCP_Array() As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Options_Precedence_Array() As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Options_TCP_Array() As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private Options_TOS_Array() As String
	#tag EndProperty

	#tag Property, Flags = &h0
		Receive_ACL_Class As Receive_ACLClass
	#tag EndProperty

	#tag Property, Flags = &h0
		SourceIPPortArray() As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private SourceWCorLogArray() As String
	#tag EndProperty

	#tag Property, Flags = &h21
		Private SourceWildcardMaskArray() As String
	#tag EndProperty

	#tag Property, Flags = &h0
		TCPProtocolPortArray() As String
	#tag EndProperty

	#tag Property, Flags = &h0
		UDPProtocolPortArray() As String
	#tag EndProperty


#tag EndWindowCode

#tag Events NextButton
	#tag Event
		Sub Action()
		  // Perform a Final Save
		  mSaveComboBoxWorkAround
		  mSaveEntriesToClass
		  CiscoACLWizardWindow.Width = 824
		  CiscoACLWizardWindow.MaxWidth =824
		  CiscoACLWizardWindow.MinWidth = 824
		  CiscoACLWizardWindow.Height = 636
		  CiscoACLWizardWindow.MaxHeight = 636
		  CiscoACLWizardWindow.MinHeight = 636
		  Self.Visible = False
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_Extended_ACLConfig1.Visible = True
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_Extended_ACLConfig1.mCreate_ACL_Configuration
		  // Hide DropACL Here Canvas
		  CiscoACLWizardWindow.DropACLCanvas.Visible = False
		  
		  // Resets the Standard ACL array since we are at the Extended Config Final Screen
		  // This protects us from a user going back-back and choosing Standard
		  Redim CiscoACLWizardWindow.IPStandardACL_Storage_Array(-1)
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.mPrepareFirstEntry
		  //Reset all Options to blank or default selection
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_StandardACL1.mClearControls
		  
		  // Enable Copy/Configuration Menu Feature
		  CiscoACLWizardWindow.CopyConfigurations.AutoEnable = True
		  // Enable Printing
		  CiscoACLWizardWindow.PrintConfigurations.AutoEnable = True
		  //Enable Save ACL
		  CiscoACLWizardWindow.SaveACL.AutoEnable = True
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CancelButton
	#tag Event
		Sub Action()
		  CiscoACLWizardWindow.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events BackButton
	#tag Event
		Sub Action()
		  // Hide DropACL Here Canvas
		  CiscoACLWizardWindow.DropACLCanvas.Visible = False
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.Visible = False
		  CiscoACLWizardWindow.CiscoACLWizard_Start1.Visible = True
		  CiscoACLWizardWindow.Width = 598
		  CiscoACLWizardWindow.MaxWidth = 598
		  CiscoACLWizardWindow.MinWidth = 598
		  
		  CiscoACLWizardWindow.Height = 338
		  CiscoACLWizardWindow.MinHeight = 338
		  CiscoACLWizardWindow.MaxHeight = 338
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events CIscoIOS_Extended_ACL_Number
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Global_ACL_Number = Self.CiscoIOS_Extended_ACL_Number.Text
		  End if
		End Sub
	#tag EndEvent
	#tag Event
		Sub Change()
		  if Me.text = "" Then
		    // Disable Decision menus
		    DenyPermitPopupMenu.Enabled = False
		    SourceIPProtocolPopUpMenu.Enabled = False
		    LogACLHit_Popupmenu.Enabled = False
		  else
		    DenyPermitPopupMenu.Enabled = True
		    SourceIPProtocolPopUpMenu.Enabled = True
		    LogACLHit_Popupmenu.Enabled = True
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SourceHostIPComboBox
	#tag Event
		Sub Change()
		  me.DeleteAllRows
		  for i as integer = 0 to UBound(SourceWCorLogArray)
		    me.addRow SourceWCorLogArray(i)
		  Next i
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  if Me.Text = "" Then
		    NextButton.Enabled = False
		  Else
		    NextButton.Enabled = True
		  End if
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  Tooltip.Hide
		  Select Case key
		  Case Chr(32)
		    Tooltip.Show("Sorry, Spaces not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(33)
		    Tooltip.Show("Sorry, Exclamation mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(34)
		    Tooltip.Show("Sorry, Quotation marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(35)
		    Tooltip.Show("Sorry, Pound sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(36)
		    Tooltip.Show("Sorry, Dollar sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(37)
		    Tooltip.Show("Sorry, Percentage sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(38)
		    Tooltip.Show("Sorry, Ampersand not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(39)
		    Tooltip.Show("Sorry, Apostrophe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(40)
		    Tooltip.Show("Sorry, Open Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(41)
		    Tooltip.Show("Sorry, Closed Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(42)
		    Tooltip.Show("Sorry, Asterisk not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(43)
		    Tooltip.Show("Sorry, Plus signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(44)
		    Tooltip.Show("Sorry, Commas not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(45)
		    Tooltip.Show("Sorry, Hyphen not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 46 (Period)
		    
		  Case Chr(47)
		    Tooltip.Show("Sorry, no Back Slashes allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 48 - 57 (Numbers)
		    
		  Case Chr(58)
		    Tooltip.Show("Sorry, Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(59)
		    Tooltip.Show("Sorry, Semi Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(60)
		    Tooltip.Show("Sorry, Less than bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(61)
		    Tooltip.Show("Sorry, Equal sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(62)
		    Tooltip.Show("Sorry, Greater than bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(63)
		    Tooltip.Show("Sorry, Question Mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(64)
		    Tooltip.Show("Sorry, ""At"" signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    //Case Chr(65) to Chr(90)
		    //Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    //Return True
		    
		  Case Chr(91)
		    Tooltip.Show("Sorry, Open Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(92)
		    Tooltip.Show("Sorry, Back Slash not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(93)
		    Tooltip.Show("Sorry, Closed Square Bracket not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(94)
		    Tooltip.Show("Sorry, Caret not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(95)
		    Tooltip.Show("Sorry, Underscore not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(96)
		    Tooltip.Show("Sorry, Tilde not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    //Case Chr(97) to Chr(122)
		    //Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    //Return True
		    
		  Case Chr(123)
		    Tooltip.Show("Sorry, Open Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(124)
		    Tooltip.Show("Sorry, Pipe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(125)
		    Tooltip.Show("Sorry, Closed Curly Brackets not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(126)
		    Tooltip.Show("Sorry, Grave Accent Marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(127)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		  End Select
		  
		  If key = Chr(9) Or key = Chr(13) Then
		    // Only Auto Guess Inverse Mask on Subnet type since Host is always 0.0.0.0
		    if SourceTypePopUpMenu.Text = "Subnet" Then
		      Dim GetWildcardMask as String = fSubnet_AutoInverseMask(SourceHostIPComboBox)
		      SourceWildcardMaskComboBox.Text = GetWildcardMask
		    End if
		    
		    if Me.Text <> "" Then
		      SourceWCorLogArray.Append me.Text
		    End if
		    // IP validation
		    Dim IPV4_Validation as String = fIPv4_Validation(me.Text)
		    Select Case IPV4_Validation
		    Case "Not_Valid"
		      Tooltip.Show("Please Enter a Valid IPv4 Address", System.MouseX, System.MouseY, True)
		      // Check to see if we Enable or Disable the Continue Button
		      Me.Text = ""
		      NextButton.Enabled = False
		      Return True
		    Else
		      NextButton.Enabled = True
		    End Select
		  End if
		  
		  
		  
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub MouseMove(X As Integer, Y As Integer)
		  ToolTip.Hide
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseUp(X As Integer, Y As Integer) As Boolean
		  if Me.Text = "" Then
		    Exit
		  Elseif Me.Text <> "" Then
		    SourceWCorLogArray.Append me.Text
		  End if
		  
		  // IP validation
		  Dim IPV4_Validation as String = fIPv4_Validation(me.Text)
		  Select Case IPV4_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid Source IP Address", System.MouseX, System.MouseY, True)
		    Me.Text = ""
		    NextButton.Enabled = False
		    Return True
		  Else
		    NextButton.Enabled = True
		  End Select
		End Function
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  AutoSaved_Canvas.Visible = True
		  
		  
		  if Me.Text <> "" Then
		    SourceWCorLogArray.Append me.Text
		  End if
		  // IP validation
		  Dim IPV4_Validation as String = fIPv4_Validation(me.Text)
		  Select Case IPV4_Validation
		  Case "Not_Valid"
		    Tooltip.Show("Please Enter a Valid Source IP Address", System.MouseX, System.MouseY, True)
		    Me.Text = ""
		    NextButton.Enabled = False
		  Else
		    NextButton.Enabled = True
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  Return True
		End Function
	#tag EndEvent
#tag EndEvents
#tag Events SourceTypePopUpMenu
	#tag Event
		Sub Change()
		  if me.Text = "Any" Then
		    // Hide All Source Menus and labels
		    EnterSourceIPHostLabel.Visible = False
		    SourceHostIPComboBox.Enabled = False
		    SourceHostIPComboBox.Visible = False
		    SourceHostIPComboBox.Text = ""
		    SourceWildcardMaskLabel.Visible = False
		    SourceWildcardMaskComboBox.Enabled = False
		    SourceWildcardMaskComboBox.Visible = False
		    SourceWildcardMaskComboBox.Text = ""
		    
		    SourceProtocolLabel.Visible = False
		    SourcePortNumberPopUpMenu.Visible = False
		    SourceRangeEndLabel.Visible = False
		    SourceRangeEndLabelPopUpMenu.Visible = False
		    SourcePortNumberPopUpMenu.Enabled = False
		    
		    //// Newly Position Port Filter, Source Protocol and ENd Range Protocol Controls
		    SourcePortFilterLabel.Left = 166
		    SourcePortFilterLabel.Top = 164
		    SourcePortFilterPopUpMenu.Width = 133
		    SourcePortFilterPopUpMenu.Left =  166
		    SourcePortFilterPopUpMenu.Top = 184
		    SourceProtocolLabel.Left = 308
		    SourceProtocolLabel.Top = 164
		    SourcePortNumberPopUpMenu.Left = 308
		    SourcePortNumberPopUpMenu.Top = 184
		    
		    
		  Elseif me.text = "Host" Then
		    // Show Source Menus
		    SourceHostIPComboBox.Enabled = True
		    SourceHostIPComboBox.Visible = True
		    EnterSourceIPHostLabel.Visible = True
		    SourceWildcardMaskLabel.Visible = False
		    SourceWildcardMaskComboBox.Enabled = False
		    SourceWildcardMaskComboBox.Visible = False
		    
		    if SourceIPProtocolPopUpMenu.Text = "tcp" OR SourceIPProtocolPopUpMenu.Text = "udp" Then
		      SourcePortFilterLabel.Visible = True
		      SourcePortFilterPopUpMenu.Visible = True
		      SourcePortFilterPopUpMenu.Enabled = True
		      SourceProtocolLabel.Visible = True
		      SourcePortNumberPopUpMenu.Visible = True
		      if SourcePortFilterPopUpMenu.Text = "" Then
		        SourcePortNumberPopUpMenu.Enabled = False
		      End if
		    End if
		    
		    //// RE-Position Port Filter, Source Protocol and ENd Range Protocol Controls Back To Default Locations
		    SourcePortFilterLabel.Left = 308
		    SourcePortFilterLabel.Top = 164
		    SourcePortFilterPopUpMenu.Width = 116
		    SourcePortFilterPopUpMenu.Left =  308
		    SourcePortFilterPopUpMenu.Top = 184
		    
		    SourceProtocolLabel.Left = 433
		    SourceProtocolLabel.Top = 164
		    SourceProtocolLabel.Visible = True
		    SourcePortNumberPopUpMenu.Left = 433
		    SourcePortNumberPopUpMenu.Top = 184
		    SourcePortNumberPopUpMenu.Visible = True
		    SourcePortNumberPopUpMenu.Enabled = True
		    SourceRangeEndLabel.Left = 433
		    SourceRangeEndLabel.Top = 209
		    SourceRangeEndLabelPopUpMenu.Left = 433
		    SourceRangeEndLabelPopUpMenu.Top = 230
		    SourcePortFilterPopUpMenu.ListIndex = 0
		    
		    
		  Elseif me.text = "Subnet" Then
		    // Return Source Port Filter to regular position
		    Self.SourceWildcardMaskComboBox.Text = "" + EndOfLine
		    SourcePortFilterLabel.Left = 307
		    SourcePortFilterPopUpMenu.Left = 307
		    
		    SourceProtocolLabel.Left = 432
		    SourcePortNumberPopUpMenu.Left = 432
		    
		    SourceRangeEndLabel.Left = SourcePortNumberPopUpMenu.Left
		    SourceRangeEndLabelPopUpMenu.Left =  SourcePortNumberPopUpMenu.Left
		    
		    // Show Source Menus
		    EnterSourceIPHostLabel.Visible = True
		    SourceHostIPComboBox.Enabled = True
		    SourceHostIPComboBox.Visible = True
		    SourceWildcardMaskLabel.Visible = True
		    SourceWildcardMaskComboBox.Enabled = True
		    SourceWildcardMaskComboBox.Visible = True
		    
		    if SourceIPProtocolPopUpMenu.Text = "tcp" OR SourceIPProtocolPopUpMenu.Text = "udp" Then
		      SourcePortFilterLabel.Visible = True
		      SourcePortFilterPopUpMenu.Visible = True
		      SourcePortFilterPopUpMenu.Enabled = True
		      SourceProtocolLabel.Visible = True
		      SourcePortNumberPopUpMenu.Visible = True
		      if SourcePortFilterPopUpMenu.Text = "" Then
		        SourcePortNumberPopUpMenu.Enabled = False
		      End if
		      
		      //// RE-Position Port Filter, Source Protocol and ENd Range Protocol Controls Back To Default Locations
		      SourcePortFilterLabel.Left = 308
		      SourcePortFilterLabel.Top = 164
		      SourcePortFilterPopUpMenu.Width = 116
		      SourcePortFilterPopUpMenu.Left =  308
		      SourcePortFilterPopUpMenu.Top = 184
		      
		      SourceProtocolLabel.Left = 433
		      SourceProtocolLabel.Top = 164
		      SourceProtocolLabel.Visible = True
		      SourcePortNumberPopUpMenu.Left = 433
		      SourcePortNumberPopUpMenu.Top = 184
		      SourcePortNumberPopUpMenu.Visible = True
		      SourcePortNumberPopUpMenu.Enabled = True
		      SourceRangeEndLabelPopUpMenu.Left = 433
		      SourceRangeEndLabelPopUpMenu.Top = 230
		      SourcePortFilterPopUpMenu.ListIndex = 0
		    End if
		  end if
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop = "Any,Host,Subnet"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Source_Type = Self.SourceTypePopUpMenu.Text
		  End if
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DenyPermitPopupMenu
	#tag Event
		Sub Change()
		  if Me.Text <> "" Then
		    SourceIPProtocolPopUpMenu.Enabled = True
		    LogACLHit_Popupmenu.Enabled = True
		  Elseif me.Text = "" then
		    SourceIPProtocolPopUpMenu.Enabled = False
		    SourceIPProtocolPopUpMenu.ListIndex = 0
		  End if
		  
		  mContinueButtonShowOrHide
		  AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Dim DataPop as String
		  Dim Last, Counter as Integer
		  
		  DataPop = ",permit,deny"
		  Last=CountFields(DataPop,",")
		  
		  For Counter=1 to last
		    me.addRow NthField(DataPop,",",counter)
		  Next
		  
		  me.ListIndex = 0
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  AutoSaved_Canvas.Visible = False
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CentralCounter).Global_ACL_Decision = Self.DenyPermitPopupMenu.Text
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ACL_Config_New_Button
	#tag Event
		Sub Action()
		  // Increment Central Counter since we are adding a blank entry
		  mSaveComboBoxWorkAround
		  
		  CentralCounter = CentralCounter + 1
		  mClearControls
		  
		  //// Re-Instantiate New Class and Create New Class Array Enty
		  CiscoACLWizardWindow.IPExtendedACL_Storage = New IPExtendedACL_Storageclass
		  CiscoACLWizardWindow.IPExtendedACL_Storage_Array.Insert CentralCounter,CiscoACLWizardWindow.IPExtendedACL_Storage
		  mSaveEntriesToClass
		  
		  // Update Current Display Stats on ACL Setting Screen
		  mSet_Current_DisplayCounter
		  // Update UpperBounds Display Stats on ACL Setting Screen
		  mSet_Current_DisplayUpperBoundsCounter
		  
		  // Lock in ACL Number if we are past the first entry
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) > 0 Then
		    CIscoIOS_Extended_ACL_Number.Enabled = False
		  End if
		  
		  //// Shoud we Show the Prev/Next Buttons?
		  mCheckToEnableNextNavButton
		  mCheckToEnablePrevNavButton
		  mCheckToEnableDeleteEntryButton
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events AutoSaved_Canvas
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ForeColor = RGB(228,228,228)
		  g.FillRect(0,0,me.Width,me.Height)
		  g.DrawPicture(Saved,0,0)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ACL_Config_Next_Button
	#tag Event
		Sub Action()
		  mSaveComboBoxWorkAround
		  mClearControls
		  if CentralCounter+1 <= UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) Then
		    CentralCounter = CentralCounter + 1
		    // Load Values To Display
		    mLoadEntriesToScreen(CentralCounter)
		  End if
		  
		  // lock the ACL Number if we are Past the First Entry
		  if CentralCounter > 0 Then
		    CIscoIOS_Extended_ACL_Number.Enabled = False
		  End if
		  
		  // Update Display Stats on ACL Setting Screen
		  mSet_Current_DisplayCounter
		  //
		  //// Shoud we Show the Prev/Next Buttons?
		  mCheckToEnableNextNavButton
		  mCheckToEnablePrevNavButton
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ACL_Config_Prev_Button
	#tag Event
		Sub Action()
		  mSaveComboBoxWorkAround
		  mClearControls
		  if CentralCounter-1 >= 0 Then
		    CentralCounter = CentralCounter - 1
		    
		    // Load Values to Display
		    mLoadEntriesToScreen(CentralCounter)
		    
		    
		    //// Unlock the ACL Number if we are on the First Entry
		    if CentralCounter = 0 Then
		      CIscoIOS_Extended_ACL_Number.Enabled = True
		    End if
		    
		    
		    // Update Display Stats on ACL Setting Screen
		    mSet_Current_DisplayCounter
		    
		    // Shoud we Show the Prev/Next Buttons?
		    mCheckToEnableNextNavButton
		    mCheckToEnablePrevNavButton
		    //
		  End if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SourceWildcardMaskComboBox
	#tag Event
		Sub Change()
		  me.DeleteAllRows
		  for i as integer = 0 to UBound(SourceWildcardMaskArray)
		    me.addRow SourceWildcardMaskArray(i)
		  Next i
		  
		  AutoSaved_Canvas.Visible = True
		  mContinueButtonShowOrHide
		  
		  if Me.Text = "" Then
		    NextButton.Enabled = False
		  Else
		    NextButton.Enabled = True
		  End if
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.addRow ""
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  Tooltip.Hide
		  Select Case key
		  Case Chr(32)
		    Tooltip.Show("Sorry, Spaces not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(33)
		    Tooltip.Show("Sorry, Exclamation mark not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(34)
		    Tooltip.Show("Sorry, Quotation marks not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(35)
		    Tooltip.Show("Sorry, Pound sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(36)
		    Tooltip.Show("Sorry, Dollar sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(37)
		    Tooltip.Show("Sorry, Percentage sign not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(38)
		    Tooltip.Show("Sorry, Ampersand not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(39)
		    Tooltip.Show("Sorry, Apostrophe not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(40)
		    Tooltip.Show("Sorry, Open Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(41)
		    Tooltip.Show("Sorry, Closed Parenthesis not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(42)
		    Tooltip.Show("Sorry, Asterisk not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(43)
		    Tooltip.Show("Sorry, Plus signs not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(44)
		    Tooltip.Show("Sorry, Commas not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(45)
		    Tooltip.Show("Sorry, Hyphen not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 46 (Period)
		    
		  Case Chr(47)
		    Tooltip.Show("Sorry, Only Numbers and Periods allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		    // Allow 48 - 57 (Numbers)
		    
		  Case Chr(58)
		    Tooltip.Show("Sorry, Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
		  Case Chr(59)
		    Tooltip.Show("Sorry, Semi Colon not allowed", System.MouseX, System.MouseY, True)
		    Return True
		    
