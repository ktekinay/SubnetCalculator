		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_TimeRange_Name = Self.TimeRange_TextField.Text
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
