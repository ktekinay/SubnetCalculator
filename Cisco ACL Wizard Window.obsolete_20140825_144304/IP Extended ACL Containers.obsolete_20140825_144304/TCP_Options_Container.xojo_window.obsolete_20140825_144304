	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Ack_Set= Self.TCP_Ack_CheckBox.Value
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub Action()
		  // Hide Fragments option when selected
		  if Me.Value = true Then
		    TCP_Fragments_CheckBox.Enabled = False
		    
		  Elseif Me.Value = False AND TCP_Established_CheckBox.Value = False AND TCP_Urgent_CheckBox.Value = False AND TCP_Syn_CheckBox.Value = False _
		    AND TCP_Push_CheckBox.Value = False AND TCP_Reset_CheckBox.Value = False AND _
		    TCP_Fin_CheckBox.Value = False Then
		    TCP_Fragments_CheckBox.Enabled = True
		    
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
