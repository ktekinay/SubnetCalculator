		  Elseif Me.Value = False AND TCP_Established_CheckBox.Value = False AND TCP_Urgent_CheckBox.Value = False AND TCP_Syn_CheckBox.Value = False _
		    AND TCP_Reset_CheckBox.Value = False AND TCP_Ack_CheckBox.Value = False AND _
		    TCP_Fin_CheckBox.Value = False Then
		    TCP_Fragments_CheckBox.Enabled = True
		    
		  End if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TCP_QOS_OptionsPopUpMenu
	#tag Event
		Sub Change()
		  if Me.Text = "" Then
		    TCP_TOS_PopUpMenu.Enabled = False
		    TCP_TOS_PopUpMenu.ListIndex = 0
		    TCP_Precedence_PopUpMenu.Enabled  = False
		    TCP_Precedence_PopUpMenu.ListIndex = 0
		    TCP_DSCP_PopUpMenu.Enabled = False
		    TCP_DSCP_PopUpMenu.ListIndex = 0
		    
		  Elseif Me.Text = "dscp" Then
		    // Hide Other Controls First
		    TCP_TOS_PopUpMenu.Enabled = False
		    TCP_TOS_PopUpMenu.Visible = False
		    TOS_Choice_Label.Visible = False
		    TCP_Precedence_PopUpMenu.Visible = False
		    TCP_Precedence_PopUpMenu.Enabled = False
		    Prec_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreateTCP_DSCP_Array
		    TCP_DSCP_PopUpMenu.Enabled = True
		    DSCP_Choice_Label.Visible = True
		    TCP_DSCP_PopUpMenu.Visible = True
		    
		  Elseif me.Text = "precedence" Then
		    // Hide Other Controls First
		    TCP_DSCP_PopUpMenu.Enabled = False
		    TCP_DSCP_PopUpMenu.Visible = False
		    DSCP_Choice_Label.Visible = False
		    TCP_TOS_PopUpMenu.Enabled = False
		    TCP_TOS_PopUpMenu.Visible = False
		    TOS_Choice_Label.Visible = False
		    
		    // Show this Control
		    mCreateTCP_Precedence_Array
		    TCP_Precedence_PopUpMenu.Visible = True
		    TCP_Precedence_PopUpMenu.Enabled = True
		    Prec_Choice_Label.Visible = True
		    
		  Elseif me.Text = "tos" Then
		    // Hide Other Controls First
		    TCP_DSCP_PopUpMenu.Enabled = False
		    TCP_DSCP_PopUpMenu.Visible = False
		    DSCP_Choice_Label.Visible = False
		    TCP_Precedence_PopUpMenu.Visible = False
		    TCP_Precedence_PopUpMenu.Enabled = False
		    Prec_Choice_Label.Visible = False
		    // Show this Control
		    mCreateTCP_TOS_Array
		    TCP_TOS_PopUpMenu.Enabled = True
		    TCP_TOS_PopUpMenu.Visible = True
		    TOS_Choice_Label.Visible = True
		  end if
		  
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.mContinueButtonShowOrHide
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  me.AddRow ""
		  me.AddRow "dscp"
		  me.AddRow "precedence"
		  me.AddRow "tos"
		  me.ListIndex = 0
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_QOS_Type = Self.TCP_QOS_OptionsPopUpMenu.Text
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseEnter()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TCP_Reset_CheckBox
