#tag Events TCP_Fragments_CheckBox
	#tag Event
		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_Checkmark_Fragments_Set= Self.TCP_Fragments_CheckBox.Value
		  End if
		  CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.AutoSaved_Canvas.Visible = True
		End Sub
	#tag EndEvent
	#tag Event
		Sub Action()
		  // If Fragments checked than disable all other TCP Bits
		  
		  if Me.Value = true Then
		    TCP_Established_CheckBox.Enabled = False
		    TCP_Syn_CheckBox.Enabled = False
		    TCP_Push_CheckBox.Enabled = False
		    TCP_Reset_CheckBox.Enabled = False
		    TCP_Ack_CheckBox.Enabled = False
		    TCP_Fin_CheckBox.Enabled = False
		    TCP_Urgent_CheckBox.Enabled = False
		    
		  Elseif Me.Value = False Then
		    TCP_Established_CheckBox.Enabled = True
		    TCP_Syn_CheckBox.Enabled = True
		    TCP_Push_CheckBox.Enabled = True
		    TCP_Reset_CheckBox.Enabled = True
		    TCP_Ack_CheckBox.Enabled = True
		    TCP_Fin_CheckBox.Enabled = True
		    TCP_Urgent_CheckBox.Enabled = True
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
