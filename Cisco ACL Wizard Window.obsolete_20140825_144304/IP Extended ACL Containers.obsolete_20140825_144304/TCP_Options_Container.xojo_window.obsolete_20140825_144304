		Sub MouseExit()
		  if UBound(CiscoACLWizardWindow.IPExtendedACL_Storage_Array) <> -1 Then
		    // Send to Function to Return only Port number
		    Dim QOSValue as String = fConvertToProperQOSValues(Me.Text)
		    CiscoACLWizardWindow.IPExtendedACL_Storage_Array(CiscoACLWizardWindow.CiscoACLWizard_IOS_ExtendedACL1.CentralCounter).OPTIONS_TCP_QOS_DSCP_Value = QOSValue
		  End if
		  
		End Sub
