	#tag Method, Flags = &h21
		Private Sub mCreateTCP_DSCP_Array()
		  // If Enteries are already there then don't process
		  Dim AlreadyEntered as Boolean = False
		  
		  // catch duplicate entries
		  for i as integer = 0 to UBound(Options_DSCP_Array)
		    Select Case Options_DSCP_Array(i)
		    Case "af21 (18)"
		      AlreadyEntered = True
		      exit sub
		    Case "af22 (20)"
		      AlreadyEntered = True
		      exit sub
		    Case "best effort (0)"
		      AlreadyEntered = True
		      exit sub
		    Case "ef (46)"
		      AlreadyEntered = True
		      exit sub
		    End Select
		  Next i
		  
		  If AlreadyEntered = False Then
		    Options_DSCP_Array.Append ""
		    Options_DSCP_Array.Append "best effort (0)"
		    Options_DSCP_Array.Append "ef (46)"
		    Options_DSCP_Array.Append "af11 (10)"
		    Options_DSCP_Array.Append "af12 (12)"
		    Options_DSCP_Array.Append "af13 (14)"
		    Options_DSCP_Array.Append "af21 (18)"
		    Options_DSCP_Array.Append "af22 (20)"
		    Options_DSCP_Array.Append "af23 (22)"
		    Options_DSCP_Array.Append "af31 (26)"
		    Options_DSCP_Array.Append "af32 (28)"
		    Options_DSCP_Array.Append "af33 (30)"
		    Options_DSCP_Array.Append "af41 (34)"
		    Options_DSCP_Array.Append "af42 (36)"
		    Options_DSCP_Array.Append "af43 (38)"
		    Options_DSCP_Array.Append "cs1 (8)"
		    Options_DSCP_Array.Append "cs2 (16)"
		    Options_DSCP_Array.Append "cs3 (24)"
		    Options_DSCP_Array.Append "cs4 (32)"
		    Options_DSCP_Array.Append "cs5 (40)"
		    Options_DSCP_Array.Append "cs6 (48)"
		    Options_DSCP_Array.Append "cs7 (56)"
		    
		    
		    // Load Pop Up Menu
		    for i as integer = 0 to UBound(Options_DSCP_Array)
		      TCP_DSCP_PopUpMenu.AddRow (Options_DSCP_Array(i))
		    Next i
		    
		  End if
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub mCreateTCP_Precedence_Array()
		  // If Enteries are already there then don't process
		  Dim AlreadyEntered as Boolean = False
		  
		  for i as integer = 0 to UBound(Options_Precedence_Array)
		    Select Case Options_Precedence_Array(i)
		    case "routine (0)"
		      AlreadyEntered = True
		      exit sub
		    Case "priority (1)"
		      AlreadyEntered = True
		      exit sub
		    End  Select
		  Next i
		  
		  If AlreadyEntered = False Then
		    Options_Precedence_Array.Append ""
		    Options_Precedence_Array.Append "routine (0)"
		    Options_Precedence_Array.Append "priority (1)"
		    Options_Precedence_Array.Append "immediate (2)"
		    Options_Precedence_Array.Append "flash (3)"
		    Options_Precedence_Array.Append "flash-override (4)"
		    Options_Precedence_Array.Append "critical (5)"
		    Options_Precedence_Array.Append "internet (6)"
		    Options_Precedence_Array.Append "network (7)"
		    
		    for i as integer = 0 to UBound(Options_Precedence_Array)
		      TCP_Precedence_PopUpMenu.AddRow (Options_Precedence_Array(i))
		    Next i
		    
		  End if
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub mCreateTCP_TOS_Array()
		  // If Enteries are already there then don't process
		  Dim AlreadyEntered as Boolean = False
		  
		  for i as integer = 0 to UBound(Options_TOS_Array)
		    Select Case Options_TOS_Array(i)
		    case "priority(1)"
		      AlreadyEntered = True
		      exit sub
		    case "flash (3)"
		      AlreadyEntered = True
		      exit sub
		    End Select
		  Next i
		  
		  If AlreadyEntered = False Then
		    Options_TOS_Array.Append ""
		    Options_TOS_Array.Append "routine (0)"
		    Options_TOS_Array.Append "priority(1)"
		    Options_TOS_Array.Append "immediate (2)"
		    Options_TOS_Array.Append "flash (3)"
		    Options_TOS_Array.Append "flash override (4)"
		    Options_TOS_Array.Append "critical (5)"
		    Options_TOS_Array.Append "inter-network control (6)"
		    Options_TOS_Array.Append "network control (7)"
		    
		    // Load PopUpMenu
		    for i as integer = 0 to UBound(Options_TOS_Array)
		      TCP_TOS_PopUpMenu.AddRow (Options_TOS_Array(i))
		    Next i
		    
		  End If
		End Sub
	#tag EndMethod


