#tag WindowCode
	#tag Method, Flags = &h21
		Private Function fConvertToProperQOSValues(InputPortString as String) As String
		  // Find only the proper QOS Values
		  Dim FindPortNumber_RegEx as RegEx
		  Dim FindPortNumber_RegExMatch as RegExMatch
		  Dim FindPortNumber_Value as String
		  FindPortNumber_RegEx = New RegEx
		  FindPortNumber_RegEx.Options.Greedy = False
		  FindPortNumber_RegEx.Options.caseSensitive = false
		  FindPortNumber_RegEx.SearchPattern = "(?<=[(]).+(?=[)])"
		  FindPortNumber_RegExMatch = FindPortNumber_RegEx.Search(InputPortString)
		  if FindPortNumber_RegExMatch <> nil then
		    FindPortNumber_Value = FindPortNumber_RegExMatch.SubExpressionString(0)
		  end if
		  
		  // Just in case we pass in a true number pass it through
		  if FindPortNumber_Value = "" Then
		    FindPortNumber_Value = InputPortString
		  End if
		  
		  Return FindPortNumber_Value
		End Function
	#tag EndMethod

